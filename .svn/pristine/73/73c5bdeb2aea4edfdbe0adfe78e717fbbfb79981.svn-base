          data-buildownerarchiveid="{{buildOwnerArchiveId}}"
          data-buildownermobile='{{buildOwnerMobile}}'
          data-buildownername='{{buildOwnerName}}'
          data-rentmoney='{{rentMoney}}'
          data-buymoney='{{buyMoney}}'
