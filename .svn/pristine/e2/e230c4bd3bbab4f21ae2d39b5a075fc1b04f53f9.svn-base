.around-cotent{
    background-color: #fff;
    overflow-y: auto;
    height: 250rpx;
    bottom:0;
    z-index: 999;
    width:100%;
}
.around-cnt-item{
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    font-size: 24rpx;
    border-bottom: 1px solid #eee;
    display: flex;
    overflow: hidden;
}

.around-cnt-item .left{
    text-align: left;
    width: 48%;
    overflow: hidden;
    font-size: 24rpx;
    color: #000;
    padding-left: 10rpx;
}
.around-cnt-item .right{
    text-align: right;
    width: 50%;
    overflow: hidden;
    font-size: 24rpx;
    color: #999;
    padding-right: 10rpx;
}
.noSurroundData{
  font-size: 26rpx;
