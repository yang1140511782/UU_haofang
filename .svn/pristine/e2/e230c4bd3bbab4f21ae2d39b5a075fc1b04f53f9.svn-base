page{
    height: 100%
}
#map{
    height:70%;
    width: 100%;
}
.main{
    height: 100%;
     width: 100%;
}
.nav{
    background-color:#fff;
    font-size:24rpx;
    width:100%;
    overflow-x: auto;
    overflow-y: hidden;
    padding-top: 10rpx;
    box-sizing: border-box;
}
.nav-but{
  border: cadetblue 1px solid;
  border-radius: 0px;
  width: 25%;
  text-align: center;
  line-height: 30px;
  display: inline-flex;
}
#scroll-map{
    width: 120%;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-align: center;
}
::-webkit-scrollbar{width:0px;height: 0}
.scroll-view {
	width: 100rpx;
	height: 100%;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.common{
	width: 50rpx;
	height: 50rpx;
	background-position: center center;
    background-repeat: no-repeat;
    background-size: 100% 100%;
}
.map-type{
	width:120rpx;
    height:50rpx;
    line-height:50rpx;
    font-size:24rpx;
    text-align:center;
}
