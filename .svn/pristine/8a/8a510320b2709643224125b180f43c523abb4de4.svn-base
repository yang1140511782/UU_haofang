.edit-btn{
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
  background: url(http://uuweb.haofang.net/PublicC/images/member_edit.png) center center no-repeat;
  background-size: 70% 70%;
  position: absolute;
  right: 0;
  top: -12rpx;
}
.down-app-box{
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
}
.down-mask{
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  width: 100%;
  background: rgba(0, 0, 0, 0.3);
  height: 100%;
}
.down-con{
  position: absolute;
  top: 50%;
  left: 50%;
  width: 640rpx;
  transform: translate(-50%,-50%);
  -webkit-transform: translate(-50%,-50%);
  -moz-transform: translate(-50%,-50%);
  -ms-transform: translate(-50%,-50%);
  -o-transform: translate(-50%,-50%);
  z-index: 20;
  box-sizing: border-box;
  border-radius: 5px;
  background: #fff;
}
.down-txt{
  font-size: 30rpx;
  text-align: center;
 font-weight: bold
}
.down-txt-top{
  padding-top:55rpx; 
}
.down-txt-bottom{
   padding-bottom:40rpx
}
.down-btn-txt{
  background: #17b980;
  color: #fff;
  font-size: 34rpx;
  width: 523rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin: 40rpx auto;
  border-radius: 4rpx;
}
/*个人钱包*/
.person-wallet{
  position: relative;
  background: #fff;
  width: 100%;
  height: 263rpx;
}
.wallet-tt{
  position: relative;
  width: 100%;
  height: 104rpx;
  line-height: 104rpx;
  box-sizing: border-box;
  padding: 0 35rpx 0 94rpx;
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
}
.wallet-tt::before{
  position: absolute;
  content: '';
    top: 50%;
    margin-top: -19rpx;
    left: 35rpx;
    width: 38rpx;
    height: 38rpx;
    background: url(data:image/png;base64,) center center no-repeat;
    background-size: 100% 100%;
}
.wallet-tt::after{
    position: absolute;
    content: '';
    top: 50%;
    margin-top: -12rpx;
    right: 35rpx;
    width: 13rpx;
    height: 24rpx;
    background: url(http://uuweb.haofang.net/Public/wxApp/images/mine/arrow_right.png) center center no-repeat;
    background-size: 100% 100%;
}
.wallet-cont{
  position: relative;
  display: flex;
  padding:  0 30rpx;
}
.wallet-cont-li{
  flex: 1;
  text-align: center;
}
.wallet-cont-li .cont-li-info{
  display: block;
  margin-top: 10rpx;
}
.cont-li-info text{
  color: #666;
}
.cont-li-info text:nth-of-type(1){
  font-size: 44rpx;
  color: #333;
  margin-right: 10rpx;
}
.cont-li-info text:nth-of-type(2){
  font-size: 22rpx;
}
.cont-li-desc{
  margin-top: 15rpx;
  font-size: 26rpx;
  color: #666;
}
/*个人钱包end*/

/*我要贷款*/
.loan-entry{
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 61rpx 0 34rpx;
  height: 120rpx;
  line-height: 1;
}
.loan-entry::after{
  position: absolute;
    content: '';
    top: 50%;
    margin-top: -12rpx;
    right: 35rpx;
    width: 13rpx;
    height: 24rpx;
    background: url(http://uuweb.haofang.net/Public/wxApp/images/mine/arrow_right.png) center center no-repeat;
    background-size: 100% 100%;
}
.loan-entry .icon{
  width: 40rpx;
  height: 40rpx;
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 100% 100%;
  margin-right: 22rpx;
}
.loan-entry .info{
  flex: 1;
}
.loan-entry .info .info-text{
  font-size: 30rpx;
  color: #333;
}
.loan-entry .info .info-text-des{
  margin-top: 13rpx;
  font-size: 24rpx;
  color: #999;
}
.loan-entry .tips{
  color: #ff5400;
  font-size: 26rpx;
}

