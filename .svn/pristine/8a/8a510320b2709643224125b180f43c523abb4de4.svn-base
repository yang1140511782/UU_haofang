
@import "../../template/authorizeMaskTmp.wxss";
image{
  width: 100%;display: block;
}
page{
  position: relative;
  width: 100%;
  height: 100%;
  background: #fff;
  font-family: "Helvetica Neue", Helvetica, Arial, "PingFang SC", "Hiragino Sans GB", "Heiti SC", "Microsoft YaHei", "WenQuanYi Micro Hei", sans-serif;
}
::-webkit-scrollbar{width:0px;height: 0}
#m-wrap{
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.h-box{
  display: flex;
  align-items: center;
  position: relative;
  width: 100%;
  height: 168rpx;
  background: #fff;
  padding-left: 35rpx;
  box-sizing: border-box;
}
.mine-tx{
  width: 100rpx;
  height: 100rpx;
  border-radius:100%;
  background: url(http://uuweb.haofang.net/Public/wxApp/images/mine/fang_default.png)no-repeat center;
  background-size:100%; 
  border: 2rpx solid #e2e2e2;
  overflow: hidden;
  box-sizing: border-box;
  margin-right: 20rpx;
}
.mine-name{
  font-size: 30rpx;
  color: #333333;
  font-weight: bold;
}
.up-msg{
  position: relative;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #09b277;
  height: 40rpx;
  margin-left: 15rpx;
  background: none;
}
.up-msg::after {
  border: 0;
}
.upd-icon{
  width: 23rpx;
  height: 21rpx;
  background: url(http://uuweb.haofang.net/Public/wxApp/images/mine/update_icon.png)no-repeat center;
  background-size: 100% 100%;
  margin-right: 10rpx;
}
.login-msg{
  position: relative;
  display: block;
  height: 80rpx;
  margin-left: 0rpx;
  background: none;
  border-color: #fff;
  padding-left: 5rpx;
}
.login-msg::after {
  border: 0;
}
.login-up-msg-mask{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: none;
  border-color: #fff;
}
.login-up-msg-mask::after {
  border: 0;
}

.login-msg .click-for-login{
  display:block;
  color: #424242;
  font-size: 30rpx;
  font-weight: bold;
  height: 45rpx;
  line-height: 40rpx;
  width: 100%;
  text-align:left;
}
.login-msg .more-serve{
  color: #999999;
  font-size: 22rpx;
  height: 40rpx;
  line-height: 40rpx;
}
.mine-bind{
  position: relative;
  width: 33%;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
  margin: 0 auto;
}
.phone-icon{
  position: relative;
  top: 2rpx;
  display: inline-block;
  width: 14rpx;
  height: 22rpx;
  background: url(http://uuweb.haofang.net/Public/wxApp/images/mine/phone_icon.png)no-repeat center center ;
  background-size: 100% 100%;
  margin-right: 10rpx;
}
.jg-box{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 39rpx;
  background: url(http://uuweb.haofang.net/Public/wxApp/images/mine/m_bg.png)no-repeat center center ;
  background-size: 100% 100%;
}
.mid-nav{
  display: -webkit-flex;
  width: 100%;
  height: 150rpx;
  background: #fff;
  margin-bottom: 20rpx;
  font-size: 26rpx;
  justify-content: space-around;
  padding-top: 20rpx;
  box-sizing: border-box;
}
.icon-box{
  position: relative;
  left: 0;
  right: 0;
  margin: auto;
  margin-bottom: 20rpx;
}
.id-icon{
  width: 44rpx;
  height: 52rpx;
  background: url(http://uuweb.haofang.net/Public/wxApp/images/mine/mine_id.png) center center no-repeat;
  background-size: 100% 100%; 
}
.collect-icon{
  width: 44rpx;
  height: 52rpx;
  background: url(http://uuweb.haofang.net/Public/wxApp/images/mine/collect.png) center center no-repeat;
  background-size: 100% 100%; 
}
.entrust-icon{
  width: 44rpx;
  height: 53rpx;
  background: url(http://uuweb.haofang.net/Public/wxApp/images/mine/my_entrust.png) center center no-repeat;
  background-size: 100% 100%; 
}
.chick-icon{
  width: 50rpx;
  height: 53rpx;
  background: url(http://uuweb.haofang.net/Public/wxApp/images/mine/house_check_in.png) center center no-repeat;
  background-size: 100% 100%; 
}
.nav-li{
  position: relative;
  width: 20%;
  height: 100%;
  text-align: center;
}
.cont-wraper{
  display: flex;
  flex-wrap: wrap;
  /*background: #ffffff;*/
  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/changeImg/mine_bg.png);
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin-bottom:20rpx;
  margin-top:14rpx;
}
.cont-box{
  position: relative;
  flex-shrink: 0;
  width: 33.33%;
  height: 190rpx;
  color:#666666;
  line-height: 274rpx;
  
  font-size: 26rpx;
  text-align: center;
  box-sizing: border-box;
  
  /*border-right:1rpx solid #e6e6e6;*/
}

