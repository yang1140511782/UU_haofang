/* pages/houseRegisterIndex/index.wxss */
.cam-app {
    width: 100%;
    min-height: 100%;
    position: relative;
    margin: 0 auto;
    background-color: #f6f6f6;
}
.cam-header {
    position: relative;
    padding: 34rpx 26rpx 0;
    margin: 0 auto;
    margin-bottom: 34rpx;
}
.cam-header image {
    width: 100%;
}
.cam-header-btn {
    display: block;
    position: absolute;
    color: #fff;
    font-size: 28rpx;
    width: 260rpx;
    height: 64rpx;
    line-height: 64rpx;
    bottom: 102rpx;
    left: 50%;
    margin-left: -130rpx;
    text-align: center;
    background-color: rgba(0,0,0,.6);
}
.cam-content {
    padding: 0 26rpx 40rpx;
}
.cam-content .cam-want-btn {
    position: relative;
    background-color: #fff;
    width: 100%;
    height: 164rpx;
    display: -webkit-box;
    -webkit-box-align: center;
    border-bottom: 4rpx solid #dbdbdb;
    border-radius: 8rpx;
}
.cam-content .cam-want-btn:not(:last-child) {
    margin-bottom: 22rpx;
}
.cam-li-icon {
    margin-left: 24rpx;
    margin-right: 26rpx;
    width: 100rpx;
    height: 100rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    background-position: center center;
}
.cam-icon-sale {
    background-image: url("https://uuweb.haofang.net/PublicC/images/campaign_sale_icon.png");
}
.cam-li-content {
    -webkit-box-flex: 1;
}
.cam-li-content view:nth-child(1) {
    color: #666;
    font-size: 30rpx;
}
.cam-li-content view:nth-child(2) {
    color: #999;
    font-size: 24rpx;
}
.cam-content .cam-want-btn::after {
    content: '';
    position: absolute;
    width: 16rpx;
    height: 24rpx;
    right: 30rpx;
    top: 50%;
    margin-top: -14rpx;
    background: url("https://uuweb.haofang.net/PublicC/images/campaign_right_arrow.png") center center no-repeat;
    background-size: 100% 100%;
}
.cam-icon-lease{
  background-image: url(https://uuweb.haofang.net/PublicC/images/campaign_lease_icon.png);
}
.cam-icon-index {
    background-image: url(https://uuweb.haofang.net/PublicC/images/campaign_index_icon.png);
}
.common{
    margin-bottom: 15rpx;
}
.toastmask{
  width:100%;
  height: 100%;
  background: rgba(0,0,0,.6);
  top: 0;
  left: 0;
  position: fixed;
  z-index:1;
}
.toastmask .toastbox{
  width: 638rpx;
  height: 393rpx;
  background: #ffffff;
  position: fixed;
  top:0;
  left:0;
  right:0;
  bottom:0;
  margin: auto;
  border-radius: 7rpx;
  text-align: center;
}
.toastmask .toastbox .name{
  height: 128rpx;
  line-height: 128rpx;
  color:#00af56;
  font-size: 32rpx;
  letter-spacing: 1px;
}
.toastmask .toastbox .note{
  color:#000000;
  padding:0 38rpx;
  letter-spacing: 1rpx;
  font-size: 28rpx;
}
.toastmask .toastbox .note cover-view{
  line-height: 55rpx;
  text-align: center;
}
.toastmask .toastbox .btn{
  border-top:1rpx solid #d7d7d7;
  position: absolute;
  width:100%;
  height: 93rpx;
  line-height: 93rpx;
  color:#25cc63;
  font-size: 32rpx;
  letter-spacing: 1px;
  left:0;
  bottom: 0;
}