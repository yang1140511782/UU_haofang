<!--消息提醒按钮-->
<navigator id="msgNotify" url="/pages/news/news" wx:if="{{unreadNum>0}}" open-type="switchTab">
  <view class="effect-tip heart"></view>
</navigator>
<!--消息提醒按钮end-->