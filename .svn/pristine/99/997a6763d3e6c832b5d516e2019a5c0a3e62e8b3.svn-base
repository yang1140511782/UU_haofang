    right: -30rpx;
    bottom: 24rpx;
    color: #e62c39;
    font-size: 30rpx;
}

.modal-toprize>.mp-line {
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/line.png') no-repeat;
    background-size: 100% 100%;
    width: 221rpx;
    height: 5rpx;
}

.tell-you {
    text-align: center;
    font-size: 26rpx;
    color: #818181;
}

.tell-you>text {
    color: #e62c39;
}

.modal-btn {
    text-align: center;
}

.modal-btn>button {
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/button.png') no-repeat;
    background-size: 100% 100%;
    width: 492rpx;
    height: 112rpx;
    border-radius: 41rpx;
    font-size: 32rpx;
    color: #5f2328;
    line-height: 122rpx;
    margin-top: 44rpx;
    margin-left: 73rpx;
    border: 0!important;
    position: inherit;
}

.modal-footer {
    color: #75000f;
    font-size: 22rpx;
    text-align: center;
    line-height: 45rpx;
}

.shiwu-prize {
    text-align: center;
    padding: 20rpx 0rpx;
}

.shiwu-prize>image {
