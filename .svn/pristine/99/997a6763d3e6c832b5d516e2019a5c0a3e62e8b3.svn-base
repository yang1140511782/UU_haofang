@import "../../../../template/authorizeMaskTmp.wxss";
.one-line-text {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 100%;
    display: block;
}


/* 头部 */

.top-mod {
    height: 364rpx;
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/backg1.png') no-repeat;
    background-size: 100% 100%;
    position: relative;
}
.top-back-index{
    position: absolute;width: 82rpx;height: 92rpx;top: -5rpx;left: 40rpx;
    background: url(data:image/png;base64,) center no-repeat;
    background-size: 100% 100%;
}
.top-mod>image {
    width: 101rpx;
    height: 101rpx;
    position: absolute;
    right: 18rpx;
    bottom: -36rpx;
    z-index: 10;
}

.top-text {
    background-color: rgba(0, 0, 0, 0.3);
    color: #fff;
    font-size: 28rpx;
    height: 52rpx;
    width: 144rpx;
    text-align: center;
    line-height: 52rpx;
    border-radius: 15px 0px 0px 15px;
    position: absolute;
    right: 0px;
    top: 10px;
}


/* 中部 */

.middle-mod {
    height: 720rpx;
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/backg3.png') no-repeat;
    background-size: 100% 100%;
    padding: 1px;
    position: relative;
}

.loop-info {
    padding: 0px 30rpx;
    margin: 38rpx 64rpx;
    margin-bottom: 10rpx;
    height: 60rpx;
    background-color: #f3861f;
    border-radius: 32rpx;
    color: #fff;
    line-height: 64rpx;
    font-size: 26rpx;
    overflow: hidden;
}

.loop-info-box {
    position: relative;
}

.prize-loop {
    position: relative;
    height: 400rpx;
    width: 750rpx;
}

.paomadeng-bottom {
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/loopDeng1.png') no-repeat;
    background-size: 100% 100%;
}

.paomadeng-top {
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/loopDeng2.png') no-repeat;
    background-size: 100% 100%;
}

.prize-loop>.yytop {
    position: absolute;
    top: 70rpx;
    left: 98rpx;
    width: 558rpx;
    height: 70rpx;
    z-index: 10;
}

.prize-loop>.yybottom {
    position: absolute;
    bottom: 90rpx;
    left: 98rpx;
    width: 558rpx;
    height: 70rpx;
    z-index: 10;
}

.prize-loop-conten {
    position: relative;
    width: 558rpx;
    height: 240rpx;
    background-color: #e44603;
    top: 70rpx;
    left: 97rpx;
    border-radius: 20rpx;
    z-index: 9;
    overflow: hidden;
}

.pl-conten-bord {
    height: 240rpx;
    width: 169rpx;
    background-color: #fff;
    margin-left: 14rpx;
    float: left;
    overflow: hidden;
}

.jp-list {
    position: relative;
    bottom: 0rpx;
}

.jp-list>.jp-imgbox {
    height: 240rpx;
    text-align: center;
}

.jp-list>.jp-imgbox>image {
    width: 140rpx;
    height: 140rpx;
    margin: 66rpx 0rpx;
}


/* 滚动奖品列表 */

.jp-list {
    position: relative;
}

.yaogan {
    position: absolute;
    right: 60rpx;
    bottom: 94rpx;
    height: 129rpx;
    width: 84rpx;
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/yaogan.png') no-repeat;
    background-size: 100% 100%;
}

.prize-btn {
    margin: 0 auto;
    height: 130rpx;
    width: 400rpx;
}

.prize-btn-normal {
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/btnup.png') no-repeat;
    background-size: 100% 100%;
}

.prize-btn-down {
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/btndown.png') no-repeat;
    background-size: 100% 100%;
}

.floar-jb {
    position: absolute;
    height: 133rpx;
    width: 76rpx;
    right: 0px;
    top: 40%;
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/jinbi.png') no-repeat;
    background-size: 100% 100%;
}


/* 底部 */

.bottom-mod {
    padding: 1px;
    position: relative;
    height: 474rpx;
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/backg2.png') no-repeat;
    background-size: 100% 100%;
}

.bot-title {
    position: relative;
    width: 213rpx;
    height: 59rpx;
    text-align: center;
    left: 50%;
    margin-left: -106rpx;
    top: 28rpx;
}

.bot-title>image {
    position: absolute;
    top: 0rpx;
    height: 59rpx;
    max-height: 100%;
    max-width: 100%;
}

.bot-title>.jip-text {
    color: #fff;
    font-size: 32rpx;
    position: relative;
    z-index: 11;
}

.bot-title>.jip-text-back {
    background-color: #f36113;
    position: absolute;
    z-index: 10;
    width: 160rpx;
    height: 20rpx;
    bottom: 10rpx;
    left: 25rpx;
}

.prize-bottom-list {
    width: 547rpx;
    height: 240rpx;
    margin: 0 auto;
    position: relative;
    top: 50rpx;
    overflow-x: auto;
}

.bot-img {
    background-color: #fff;
    height: 230rpx;
    width: 160rpx;
    border-radius: 10rpx;
    text-align: center;
    float: left;
    margin: 0rpx 7rpx;
}

.bot-img>.btnt-top {
    border-radius: 10rpx 10rpx 0rpx 0rpx;
    background-color: #ccc;
    color: #fff;
    font-size: 22rpx;
    height: 30rpx;
    line-height: 30rpx;
}

.bot-img>image {
    height: 160rpx;
    width: 160rpx;
}

.bot-img>.btnt-bottom {
    border-radius: 0rpx 0rpx 10rpx 10rpx;
    background-color: #e33c00;
    color: #fff;
    font-size: 24rpx;
    height: 40rpx;
    line-height: 40rpx;
}


/* 中奖弹出层 --现金红包*/

.backimg {
    position: fixed;
}

.page-modal {
    background-color: rgba(0, 0, 0, 0.6);
    background-size: 100% 100%;
    width: 100%;
    height: 100%;
    z-index: 20;
    top: 0;
    left: 0;
}

.modal-bgimg {
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/modalWindow.png') no-repeat;
    background-size: 100% 100%;
    width: 658rpx;
    height: 632rpx;
    z-index: 21;
    top: 214rpx;
    left: 50%;
    margin-left: -319rpx;
}

.close-modal {
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/colse.png') no-repeat;
    background-size: 100% 100%;
    width: 67rpx;
    height: 67rpx;
    margin: 0 auto;
    margin-top: 72rpx;
}

.prize-title {
    background: url('http://cdn.haofang.net/static/uuweb/activity/jinyin/index/CongratulationsGet.png') no-repeat;
    background-size: 100% 100%;
