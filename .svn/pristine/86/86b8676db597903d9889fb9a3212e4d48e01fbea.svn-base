    wx.reLaunch({
              url: "/pages/real_index/index"
    });
  },

  /**
   * 用户点击右上角分享
   */
  onShareAppMessage: function () {
	  var data = this.data;
 	 return{
 	 	title: options.buildId,
 	 	path: '/pages/newHouseDetail/newHouseDetail?buildId='+data.buildId+"&shareOpenId="+wx.getStorageSync('openId')
 	 }
  }
})