    var regName = e.currentTarget.dataset.regname
    wx.redirectTo({
      url: '../newHouseList/newHouseList?buildregion=' + regId +'&regname=' + regName
    });
  },

  /**
   * 点击在线聊天
   */
  goToIM() {
    var data = this.data;
    var userId = data.archiveId;   
    var caseId = data.buildId;
    wx.navigateTo({
      url: '../im/im?caseType=6&caseId=' + caseId + '&to=' + userId
    });
  },

  /**
   * 生命周期函数--监听页面初次渲染完成
   */
  onReady: function () {

  },

  /**
   * 生命周期函数--监听页面显示
