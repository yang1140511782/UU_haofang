    this.setData({
      bookToLookBox: false
    });
  },

  /**
   * 点击弹框,阻止事件冒泡
   */
  prevent() {

  },





  /**
   * 电话咨询
   */
  makeacall() {
    var tel = this.data.telephone;
    wx.makePhoneCall({
      phoneNumber: tel
    });
  },

  /**
   * 进入户型页
   */
  goTohuxing() {
    wx.navigateTo({
      url: '../allHouseType/allHouseType?buildid=' + this.data.buildId
    });
  },
