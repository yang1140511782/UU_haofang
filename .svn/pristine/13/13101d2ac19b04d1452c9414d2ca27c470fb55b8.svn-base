
page {
  width: 100%;
  height: 100%;
  font-family: "微软雅黑","Helvetica Neue",Helvetica,STHeiTi,sans-serif;
  font-size:26rpx;
  color: #333;
  background: #fff;
}
.wrap-box{
  width: 100%;
  height: 100%;
  position: relative;
}
.ti-th{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  background: #f8f8f8;
  font-size: 28rpx;
  color: #999999;
  padding-left: 26rpx;
  box-sizing: border-box;
}
.ul{
  width: 100%;
  padding-left: 26rpx;
  box-sizing: border-box;
  border-top: 2rpx solid #e9e9e9;
  margin-bottom: 40rpx;
}
.li{
  position: relative;
  width: 100%;
  height: 109rpx;
  font-size: 32rpx;
  color: #333333;
  line-height: 109rpx;
  border-bottom: 2rpx solid #e9e9e9;
}
.checkedcss button{
  color: #e64a19;
}
.checkedcss::after{
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  right: 26rpx;
  content: "";
  width: 33rpx;
  height: 24rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/fakeHouse/check_icon.png) center center no-repeat;
  background-size:100%;
}
.torule{
  font-size: 24rpx;
  text-align: center;
  color: #25b764;
  display:inline-block;
  margin-left: 20rpx;
  
}
.sub-btn{
  position: fixed;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 45rpx;
  font-size: 34rpx;
  line-height: 88rpx;
  color: #424242;
  background: #ffcd16;
  width: 650rpx;
  height: 88rpx;
  border-radius: 6rpx;
  text-align: center;
}
.li button{
  position: relative;
  display: inherit;
  text-align: left;
  border: none;
  border-color: #fff;
  padding: 0rpx;
  margin: 0rpx;
  background-color:#fff;
  font-size:32rpx;
  line-height: 109rpx;
}
button::after {
  border: 0;
} 
button::hover{
  border: 0;
  color: #424242;
  background-color:#fff;
}

.toat-btn .recall{
  height: 90rpx;
  line-height: 90rpx;
  width: 50%;
  border-right: 2rpx solid #ececec;
}

.rule-substrict{
  width:100%;
  padding:26rpx 40rpx;
  box-sizing:border-box;
  color: #aaa;
  line-height: 1.5;
  font-size: 24rpx;
}

   
.to-wrap{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  width: 100%;
  height: 100%;
}
.close-box{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}

.tost-box{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  z-index: 120;
  width: 640rpx;
  height: 760rpx;
  background: #fff;
  border-radius: 16rpx;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.tost-box-close{
  float:right;
  height:25px;
  color:#333333;
  background:url(https://uuweb.haofang.net/Public/wxApp/images/fakeHouse/close.png) right center no-repeat;
  background-size:44rpx;
  margin:30rpx;

}


.tost-box-ok-img{
  width:100%;
height:32%;
color:#333333;
background:url(https://uuweb.haofang.net/Public/wxApp/images/fakeHouse/ok_icon.png) center center no-repeat;
background-size:186rpx;


}
.tost-box-ok{
 line-height:48rpx;
font-size:32rpx;
color:#333333;
text-align:center;
margin-bottom:44rpx;
font-weight:600;
margin-top:40rpx;
}

.tost-txt{
  line-height:50rpx;
font-size:28rpx;
color:#333333;
text-align:center;
width:560rpx;
margin:0 44rpx 56rpx;
font-family: PingFang-SC-Regular,Helvetica,sans-serif;

}
.toat-btn .pull{
  font-size:38rpx;
line-height:88rpx;
color:#424242;
background:#ffcd16;
width:560rpx;
height:90rpx;
border-radius:6rpx;
text-align:center;
margin:0 auto;

}

