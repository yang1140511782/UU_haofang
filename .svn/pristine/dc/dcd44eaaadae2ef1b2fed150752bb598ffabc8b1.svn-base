.inputBox{
  position: absolute;
  height: 100%;
  width: 500rpx;
  top:0;
  right: 60rpx;
}
.inputBox input{
  width:100%;
  height: 100%;
  display: block;
  font-size: 30rpx;
  text-align: right;
}
.fot-box{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 180rpx;
  background: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.e_btn{
  position: absolute;
  left:0;
  right:0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 690rpx;
  height: 98rpx;
  line-height: 98rpx;
  text-align: center;
  color:#fff;
  background:-webkit-linear-gradient(left, rgb(255,111,46) , rgb(254,142,13));
  font-size: 34rpx;
  letter-spacing: 1px;
  border-radius: 10rpx;
}

/*滑动选择器*/
.time-toast{
  position: absolute;
  top: 0;
  z-index: 100000;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  font-family: '微软雅黑';
}
.time-box{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100%;
  text-align: center;
  font-size: 36rpx;
  background: #fff;
  border-color: red;
}
.time-b{
  border-color: #00CA85;
}
.time-title{
  position: absolute;
  top: 0;
  left:0;
  width: 100%;
  text-align: center;
  color: #00CA85;
  background: #fff;
  z-index: 100;
  height:100rpx;
  font-size:33rpx;
  line-height:85rpx;
  letter-spacing:1px;
}
.btnaa-box{
  position: absolute;
  top: 0;
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  font-size:34rpx; 
  border-bottom: 1rpx solid #e5e5e5;
  z-index: 1000;
}
.btnaa-box view{
  width: 22%;
  text-align: center;
  font-size: 34rpx;
  letter-spacing: 3px;
}
.qd-btn{
  color:#ff5400;
}
.sjbox{
  color: #333;
}

.placeHolder{
  color:#CCCCCC;
}