.to-wrap{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  width: 100%;
  height: 100%;
}
.close-box{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}
.tost-box{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  z-index: 120;
  width: 600rpx;
  height: 320rpx;
  background: #fff;
  border-radius: 16rpx;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.tost-title{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  border-bottom:2rpx solid #e7e7e7;
}
.tost-txt{
  flex: 1;
  padding:10rpx 55rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  color: #333333;
  text-align: center;
}
.toat-btn{
  width: 100%;
  height: 90rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 90rpx;
  display: flex;  
  flex-direction: row;
  justify-content: center;
}
.toat-btn .recall{
  height: 75rpx;
  line-height: 75rpx;
  width: 35%;
  background: #ff5400;
  color: white;
  border-radius: 16rpx;
  margin-right:40rpx;
}
.toat-btn .pull{
  width: 35%;
  height: 75rpx;
  line-height: 75rpx;
  color: #ff5400;
  border: 2rpx solid #ff5400;
  border-radius: 16rpx;
  margin-left:40rpx;
}
.rule-substrict{
  width:100%;
  padding:26rpx 40rpx;
  box-sizing:border-box;
  color: #aaa;
  line-height: 1.5;
  font-size: 24rpx;
}
.index-btn{
  width:100rpx;
  height: 100rpx;
  background: url('http://uuweb.haofang.net/Public/wxApp/images/personalCenter/index-btn.png?t=33') center center no-repeat;
  background-size: 100% 100%;
  position: fixed;
  right: 2rpx;
  bottom:100rpx;
  z-index: 999
}
/*经纪人最后一条消息*/
.lastMsg{
  margin-bottom:10rpx;
}
.msg-box{
  background:#f7f7f9;
  border-radius:10rpx;
  width:72.5%;
  margin-left:144rpx;
  padding:13rpx;
  position:relative;
}
.arrows{
  position:absolute;
  width:30rpx;
  height:15rpx;
  top:-32rpx;
  left:20rpx;
}
.arrows image{
  width:100%;
  height:100%;
}
.msg{
  font-size:30rpx;
  margin-left:10rpx;
  color:#666666;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.send-time{
  color:#4daaf0;
  font-size:25rpx;
}
.redDot{
  position:absolute;
  top:20rpx;
  right:-10rpx;
  min-width:30rpx;
  height:30rpx;
  line-height:30rpx;
  margin-top:-30rpx;
  font-weight:bold;
  font-size:20rpx;
  color:white;
  text-align:center;
  background:#ff3d43;
  border:none;
  border-radius:100%;
  display:inline-block;
}

/*经纪人最后一条消息end*/
