}
.no-order{
  width: 44rpx;
  height: 44rpx;
  background: url(http://cdn.haofang.net/static/uuminiapp/entrustImg/no_order_icon.png) no-repeat center center;
  background-size: 100%;
  margin-bottom: 10rpx;
}
.tj-icon{
  position: relative;
  width: 44rpx;
  height: 44rpx;
  background: #1bc964;
  margin-bottom: 10rpx;
  border-radius: 100%;
}
.tj-icon image{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 100%;
  height: 100%;
}
.no-tj{
  position: relative;
  width: 44rpx;
  height: 44rpx;
  background: #f4f4f4;
  border-radius: 100%;
  background-size: 100%;
  margin-bottom: 10rpx;
}
.no-tj image{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 24rpx;
  height: 28rpx;
}
.no-dk{
  position: relative;
  width: 44rpx;
  height: 44rpx;
  background: #f4f4f4;
  border-radius: 100%;
  background-size: 100%;
  margin-bottom: 10rpx;
}
.no-dk image{
  width: 26rpx;
  height: 25rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}
.dk-icon{
  position: relative;
  width: 44rpx;
  height: 44rpx;
  background: #1bc964;
  margin-bottom: 10rpx;
  border-radius: 100%;
}
.dk-icon image{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}
.no-cj{
  width: 44rpx;
  height: 44rpx;
  background: url(http://cdn.haofang.net/static/uuminiapp/entrustImg/no_cj.png) no-repeat center center;
  background-size: 100%;
  margin-bottom: 10rpx;
}
.cj-icon{
  width: 44rpx;
  height: 44rpx;
  background: url(http://cdn.haofang.net/static/uuminiapp/new_images/cj_icon.png) no-repeat center center;
  background-size: 100%;
  margin-bottom: 10rpx;
  
}
.line-w{
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  top: 45rpx;
  width: 70%;
  height: 2rpx;
  background: #b7b7b7;
}
.line-w-a{
  position: absolute;
  height: 1rpx;
  left: 0;
  top: 25rpx;
  background: green;
}
.progress-1 .condition-box .line-w-a{
  width:0%;
  background: #4daaf0;
} 
.progress-2 .condition-box .line-w-a{
  width:35%;
  background: #4daaf0;
} 
.progress-3 .condition-box .line-w-a{
  width:65%;
  background: #4daaf0;
} 
 .progress-4 .condition-box .line-w-a{
  width:95%;
  background: #4daaf0;
} 
.condition{
  position: relative;
  z-index: 3;
}
.progress-1 .condition:nth-of-type(1),
.progress-2 .condition:nth-of-type(1),
.progress-2 .condition:nth-of-type(2),
.progress-3 .condition:nth-of-type(1),
.progress-3 .condition:nth-of-type(2),
.progress-3 .condition:nth-of-type(3),
.progress-4 .condition:nth-of-type(1),
.progress-4 .condition:nth-of-type(2),
.progress-4 .condition:nth-of-type(3),
.progress-4 .condition:nth-of-type(4)
{
  color:#4daaf0;
}
