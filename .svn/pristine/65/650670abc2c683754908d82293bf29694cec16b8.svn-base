  position:relative;
  width: 100%;
  height:120rpx;
  background: #fff;
  padding-top: 20rpx;
}
.condition-box{
    position: relative;
    right: 0;
    left: 0;
    margin: auto;
    display: flex;
    justify-content: space-between;
    width: 80%;
}
.manager-progress .condition{
  font-size: 26rpx;
  color:#b7b7b7;
  position:relative;
  padding-top: 5rpx;
  text-align: center;
  letter-spacing: 1px;
}
.order-icon{
  width: 44rpx;
  height: 44rpx;
  background: url(http://cdn.haofang.net/static/uuminiapp/new_images/order_icon.png) no-repeat center center;
  background-size: 100%;
