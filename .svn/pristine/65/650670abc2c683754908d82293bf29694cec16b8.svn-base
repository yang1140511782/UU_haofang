/* pages/entrustDetail/entrustDetail.wxss */
page{
  background: #f6f6f6;
  font-family: '微软雅黑';
  width: 100%;
  height: 100%;
}
#app{
  width: 100%;
  height: 100%;
}
/*顶部*/
.top-box{
  /* position: fixed; */
  width: 100%;
  left: 0;
  top: 0;
  z-index: 20;
  background:#fff;
}
.broker-info{
  background: #fff;
  display: flex;
  justify-content: flex-start;
  position:relative;
  border-top:1px solid #d5d5d5;
}
.icon-wrap{
  position:absolute;
  right: 40rpx;
  top:21rpx;
  width:180rpx;
  height: 70rpx;
  display: flex;
  justify-content: flex-end;
}
.icon-wrap view{
    width: 70rpx;
    height: 70rpx;
    overflow: hidden;
}
.im_icon{
    margin-right: 20rpx;
}
.icon-wrap view image{
    width:100%;
    height: 100%;
}
.pictx{
  width: 100rpx;
  height: 100rpx;
  position: relative;
  border-radius:50%;
  border: 2rpx solid #eeeeee;
  margin:0 20rpx; 
  margin-top:20rpx;
  margin-bottom: 10rpx; 
}
.pictx image{
  width: 100%;
  height: 100%;
  border-radius:50%;
  border: 0;
}
.broker-name{
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 2px;
  line-height: 46rpx;
  padding-top:20rpx;
  font-weight: bold;
}
.broker-name text{
  font-size: 30rpx;
  color: #999999;
}
/*******************评分************************/
.refuse-box{
  position: relative;
  top: 5rpx;
  display: inline-block;
  color: #999;
  font-size: 28rpx;
  font-weight: normal;
}
.refuse-icon{
  display: inline-block;
  width: 27rpx;
  height: 28rpx;
  background: url(http://cdn.haofang.net/static/uuminiapp/entrustImg/no_icon.png) no-repeat;
  background-size: 100% 100%;
  margin-left: 20rpx;
  margin-right: 10rpx;
}
.pf-box{
  display: -webkit-flex;
  align-items: center;
  height: 40rpx;
  padding-top: 7rpx;
}
.pinf{
    width: 145rpx;
    height: 24rpx;
    background: url(https://weidian.haofang.net/wxApp/images/vipPublish/star.png) no-repeat;
    background-size: 100% auto;
}
.fens{
    font-size: 16rpx;
    color:#fff;
    background-color: #fcad08;
    border-radius: 4rpx;
    padding:0 8rpx;
    margin:0 10rpx;
}
.tradeNum{
  font-size: 24rpx;
  color:#4a4949;
  padding:0 5rpx;
  border-radius: 4rpx;
}
.entrust{
  position:absolute;
  right: 40rpx;
  top:28rpx;
  width:260rpx;
}
.entrust-info{
  display: flex;
  justify-content: flex-end;
  align-items:center;
}
.comm-key{
  font-weight:normal;
  font-size:28rpx;
  color:#999;
}
.comm-num{
 font-size:36rpx;
  color: #ff5d2f;
  margin-left: 5rpx;
}
.comm-unit{
  font-size:28rpx;
  color: #ff5d2f;
}
.entrust-time{
  color:#999;
  font-size:24rpx;
  font-weight:normal;
  text-align: right;
}
.star-value-5,.star-value-5-0{background-position:0 0}
.star-value-4-1,.star-value-4-2,.star-value-4-3,.star-value-4-4,.star-value-4-5,.star-value-4-6,.star-value-4-7,.star-value-4-8,.star-value-4-9{background-position:0 -28rpx;}
.star-value-4,.star-value-4-0{background-position:0 -56rpx;}
.star-value-3-1,.star-value-3-2,.star-value-3-3,.star-value-3-4,.star-value-3-5,.star-value-3-6,.star-value-3-7,.star-value-3-8,.star-value-3-9{background-position:0 -84rpx;}
.star-value-3,.star-value-3-0{background-position:0 -112rpx;}
.star-value-2-1,.star-value-2-2,.star-value-2-3,.star-value-2-4,.star-value-2-5,.star-value-2-6,.star-value-2-7,.star-value-2-8,.star-value-2-9{background-position:0 -140rpx;}
.star-value-2,.star-value-2-0{background-position:0 -168rpx;}
.star-value-1-1,.star-value-1-2,.star-value-1-3,.star-value-1-4,.star-value-1-5,.star-value-1-6,.star-value-1-7,.star-value-1-8,.star-value-1-9{background-position:0 -196rpx;}
.star-value-1,.star-value-1-0{background-position:0 -224rpx;}
.star-value-0-1,.star-value-0-2,.star-value-0-3,.star-value-0-4,.star-value-0-5,.star-value-0-6,.star-value-0-7,.star-value-0-8,.star-value-0-9{background-position:0 -252rpx;}
.star-value-0,.star-value-0-0{background-position:0 -280rpx;}



.chooseTab{
  height: 72rpx;
  line-height: 72rpx;
  box-sizing: border-box;
  background:#fff;
  border-top:1px solid #d5d5d5;
  display: flex;
  justify-content: center;
  font-size: 24rpx;
  color:#808080;
  border-bottom:1px solid #e5e5e5;
}
.chooseTab .left{
  margin-right: 180rpx;
}
.chooseTab .active{
  color:#1bc964;
  border-bottom: 2px solid #1bc964;
}

.deligateDetail{
  background:#f8f8f8;
  height:900rpx;
  padding-top:40rpx;
  position:relative;
  box-sizing: border-box;
}
.deligateDetail .greenLine{
  position:absolute;
  height: 100%;
  left: 40rpx;
  top:40rpx;
  border-left:1px dotted #00ae3f;
  width:1px;
}
.deligateDetail .path{
  padding-left:80rpx;
  font-size: 22rpx;
  position:relative;
}
.deligateDetail .path:after{
  content:'';
  position:absolute;
  width:17rpx;
  height:17rpx;
  border:1px solid #00c06d;
  box-sizing: border-box;
  border-radius:50%;
  left:34rpx;
  top:5rpx;
}

/* 委托进度流程 */
