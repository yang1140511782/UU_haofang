/* 委托进度流程 */

.recommendHouse{
  background:#fff;
  margin-top: 10rpx;
}
.cuccessEvaluate{
  height: 79rpx;
  border-top: 1rpx solid #eee;
  line-height: 79rpx;
  text-align: center;
  font-size: 25rpx;
  color:#999999;
  background-color: #fff;
  letter-spacing: 1rpx;
}
.agreeToLookBox{
  display: flex;
  align-items: center;
  font-size: 26rpx;
  line-height: 100rpx;
  height: 100rpx;
  color:#999999;
  border-top: 1rpx solid #eee;
  justify-content:space-between;
  background:#fff;
  padding:0 30rpx;
  letter-spacing: 1px;
}
.agreeToLookBox .btn{
  width:150rpx;
  height:60rpx;
  line-height: 60rpx;
  border-radius: 7rpx;
  background: #25b764;
  color:#fff;
  font-size: 26rpx;
  text-align: center;
}
.agreeToLookBox .disagree{
  border:1px solid #404040;
  color:#404040;
  line-height: 50rpx;
  background: url(https://uuweb.haofang.net/PublicC/images/jujue.png) no-repeat 16rpx center;
  background-size: 30rpx 30rpx;
}
.agreeToLookBox .agree{
  color:#fff;
  background: url(https://uuweb.haofang.net/PublicC/images/tongyi.png) #1bc964 no-repeat 16rpx center;
  background-size: 30rpx 30rpx;
}
.agreeSuccess{
  font-size: 30rpx;
  color:#b2b2b2;
}

/*内容部分*/
.main-content{
  padding-top: 20rpx;
  height: 100%;
  box-sizing: border-box;
}

/*推荐房源列表*/
.house-section{
    overflow: hidden;
    border-bottom:15rpx solid #EFEFEF;
}
.houseItem{
  position: relative;
  display: flex;
  padding-left: 30rpx;
  padding-bottom: 29rpx;
  background-color: #fff;
}
.houseItem:not(:first-child){
  margin-top: 10rpx;
}
.houseItem .picbox{
  margin-top:42rpx;
  width:210rpx;
  height: 157rpx;
  position:relative;
  background: url(http://cdn.haofang.net/static/uuminiapp/default/list_deafault.png?t=20180606) no-repeat center center;
  background-size: cover;
}
.thumb{
  width:210rpx;
  height: 157rpx;
}
.houseItem .infobox{
  padding-left: 33rpx;
  padding-top:36rpx;
  width:445rpx;
  position: relative;
}
.houseItem .infobox .housetitle{
  font-size: 28rpx;
  font-weight: bold;
  color:#555555;
  line-height: 38rpx;
  margin-bottom: 5rpx;
  letter-spacing: 1px;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.houseItem .infobox .text{
  font-size: 24rpx;
  line-height: 1;
  color:#888888;
  padding-bottom: 15rpx;
  padding-top:10rpx;

}
.houseItem .infobox .tag{
  line-height: 1;
}
.houseItem .infobox .tag text{
  font-size: 22rpx;
  letter-spacing: 1px;
  display: inline-block;
  color:#1bc964;
  background: #e0f7ea;
  padding:5rpx 8rpx;
  padding-bottom: 6rpx;
  border-radius: 3rpx;
  margin-right:10rpx;
}
.houseItem .infobox .tag text:nth-child(4n+2){
  color:#00a0e9;
  background: #d9f1fc;
}
.houseItem .infobox .tag text:nth-child(4n+3){
  color:#ff9600;
  background: #ffeed8;
}
.houseItem .infobox .tag text:nth-child(4n+4){
  color:#f46f66;
  background: #fdeae9;
}
.houseItem .infobox .price{
  font-size: 30rpx;
  color:#ff5a1f;
  font-weight: bold;
  line-height: 61rpx;
}
.houseItem .infobox .price text{
  color:#999999;
  font-weight: normal;
  font-size: 22rpx;
  margin-left: 18rpx;
}
.houseItem .infobox .top{
  font-size: 16rpx;
  letter-spacing: 1px;
  color:#a5a5a5;
  padding:6rpx;
  border: 1px solid #dcdcdc;
  border-radius: 3rpx;
  position: absolute;
  line-height: 1;
  right:0;
  bottom: 20rpx;
}
.houseItem .house-status-tag{
  position: absolute;
  top: 33rpx;
  right: 22rpx;
  width: 130rpx;
  height: 105rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center center;
}

.tag-1{background-image: url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/refuse_house.png);}
.tag-2{background-image: url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/fake_house.png);}
.tag-3{background-image: url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/inappropriate_house.png);}
.tag-4,.tag-6,tag-7{background-image: url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/down_house.png);}
.tag-5{background-image: url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/candel_house.png);}
/*.tag-6{background-image: url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/knockdown_house.png);}*/
/*房源进度*/
.house-progress{
    border-top: 1px solid #eee;
    box-sizing: border-box;
}
.house-progress-text{
    text-align: center;
    color: #b7b7b7;
    font-size: 28rpx;
    padding: 16rpx 0;
}
.house-progress-btn{
    padding-top: 16rpx;
    padding-bottom: 16rpx;
    background-color: #FFF;
    text-align: center;
}

.house-btn-li{
    position: relative;
    line-height: 56rpx;
    font-size: 28rpx;
    width: 196rpx;
    height: 52rpx;
    line-height: 52rpx;
    margin-right: 24rpx;
    box-sizing: border-box;
    display: inline-block;
    border-radius: 8rpx;
    text-align: center;
}

.see-evaluate::before{
    content: '';
    display: inline-block;
    width: 24rpx;
    height: 24rpx;
    margin-bottom: -.2rpx;
    margin-right: 10rpx;
    background-position: center center;
    background-repeat: no-repeat;
    background-size: 100% 100%;
}
.see-evaluate{
    background: #4daaf0;
    color: #fff;
    
}
.agree-see-house{
    background-color: #4daaf0;
    color: #fff;
}
.disagree-see-house{
    background-color: #dcdcdc;
    color: rgb(102,102,102);
}
.fee-pay,.server-evaluate{
    background:#4daaf0;
    color: #fff;
}
.agree-see-house::before{background-image: url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/ic_entrust_agree.png);}
.see-evaluate::before{background-image: url(http://cdn.haofang.net/static/uuminiapp/new_images/ic_entrust_evaluation.png?t=1111);}
.disagree-see-house::before{background-image: url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/ic_entrust_cancel.png);}
.fee-pay::before{background-image: url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/ic_entrust_pay.png);}

/*推荐房源列表end*/

/*委托跟进*/
.entrust-follow{
  position: relative;
  background-color: #fff;
  margin-top: 10rpx;
}
.follow-tt-box{
  height: 108rpx;
  line-height: 108rpx;
  font-size: 32rpx;
  color: #000;
  display: -webkit-flex;
  padding-left: 32rpx;
  box-sizing: border-box;
}
.follow-tt{
  -webkit-box-flex: 1;
  width: 100%;
}
.follow-more-btn{
  position: relative;
  font-size: 24rpx;
  color: #b7b7b7;
  width: 200rpx;
  text-align: right;
  margin-right: 32rpx;
}
.follow-more-btn::after{
  content: '';
  display: inline-block;
  width: 15rpx;
  height: 9rpx;
  margin-left: 8rpx;
  margin-bottom: 6rpx;
  background: url(http://cdn.haofang.net/static/uuminiapp/changeImg/down_icon.png) center center no-repeat;
  background-size: 100% 100%;
  
}
.show-follow-detail .follow-more-btn::after{-webkit-transform:scaleY(-1);}

.follow-list{
  position: relative;
  padding-left:32rpx;
  box-sizing: border-box;
}
.follow-ul{
  position: relative;
  padding-top: 68rpx;
  padding-right: 32rpx;
  border-top: 1rpx solid #ddd;
}
.follow-li{
  position: relative;
  width: 100%;
  overflow: hidden;
  display: -webkit-box;
}
.follow-li-left{
  display: flex;
  flex-direction:column; 
  padding: 0 22rpx;
  position: relative;
}
.follow-li-left .follow-img{
  width: 64rpx;
  height: 64rpx;
  border-radius: 50rpx;
  overflow: hidden;
  background: url(http://cdn.haofang.net/static/uuminiapp/mine/fang_default.png) center center no-repeat;
  background-size: 100% 100%;
}
.follow-li-left .follow-img image{
  width: 100%;height: 100%;
}
.follow-li-left .follow-line{
  flex: 1;
  width: 2rpx;
  margin: 8rpx auto;
  background-color: #ddd;
}
.follow-li-right{
  font-size: 24rpx;
  -webkit-box-flex: 1;
}
.follow-li-right .follow-li-tt{
  margin-bottom: 30rpx;

}
.follow-li-right .follow-li-detail{
  white-space: normal;
  color: #888;
  margin-bottom: 40rpx;
}
.follow-li:first-child .follow-li-right .follow-li-tt{
  color: #4daaf0;
}
.follow-li:first-child .follow-li-right .follow-li-detail{
  color: #000;
}
/*委托跟进end*/

/*  服务评价  */
.evaluate_mask{
