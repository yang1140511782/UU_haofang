/*  立即联系弹框  */
.ddc_contact {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 9999999999;
}
.ddc_contact_content {
    position: absolute;
    left: 50%;
    top: 50%;
    width: 90%;
    height: 400rpx;
    background-color: #fff;
    z-index: 1000;
    -webkit-border-radius: 4px;
    border-radius: 4px;
    text-align: center;
    font-size: 24rpx;
    color: rgb(60,60,60);
    max-width: 640px;
    margin: 0 auto;
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
}
.ddc_contact_mc {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 999;
    background-color: rgba(0,0,0,.6);
}
.ddc_contact_content .h2 {
    text-align: center;
    font-size: 32rpx;
    height: 160rpx;
    line-height: 160rpx;
    position: relative;
    color: #000;
}
.ddc_contact_content .ol {
    padding: 0;
    display: flex;
}
.ui-row-flex .ui-col {
    flex:1;
}
.ddc_contact_content .li .span {
    display: inline-block;
    color: #333;
}
.ddc_contact_content .h2:before {
    content: "";
    display: block;
    width: 22%;
    height: 1px;
    position: absolute;
    top: 50%;
    left: 60rpx;
    background-color: #bbb;
}
.ddc_contact_content .h2:after {
    content: "";
    display: block;
    width: 22%;
    height: 1px;
    position: absolute;
    top: 50%;
    right: 60rpx;
    background-color: #bbb;
}
.ddc_contact_icon {
    box-sizing: border-box;
    width: 100%;
    height: 90rpx;
    background-size: 90rpx!important;
    margin-bottom: 20rpx;
}
