/**有新消息右侧按钮提醒**/
#msgNotify {
  width:100rpx;
  height: 100rpx;
  position: fixed;
  background: url('https://uuweb.haofang.net/Public/wxApp/images/im/news_active_new_a.png') center center no-repeat;
  right:21rpx;
  bottom:300rpx;
  border-radius: 50%;
  z-index: 9;
  background-size: 100% 100%;
}
.effect-tip {
    background-color: red;
    height: 12rpx;
    width: 12rpx;
    right:20rpx;
    top:26rpx;
    position: absolute;
    border-radius: 50%;
}

.heart {
    animation: pound 1s infinite;
    -webkit-animation: pound 1s infinite;
    -moz-animation: pound 1s infinite
}
@-webkit-keyframes pound{from{-webkit-transform:none}50%{-webkit-transform:scale(1.6)}to{-webkit-transform:none}}

@keyframes pound{from{transform:none}50%{transform:scale(1.6)}to{transform:none}}