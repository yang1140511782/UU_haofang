image{width: 100%;display: block;}

page{
  position: relative;
  width: 100%;
  height: 100%;
  background: #fff;
  font-family: "Helvetica Neue", Helvetica, Arial, "PingFang SC", "Hiragino Sans GB", "Heiti SC", "Microsoft YaHei", "WenQuanYi Micro Hei", sans-serif;
}
::-webkit-scrollbar{width:0px;height: 0}
#account-wrap{
  display: -webkit-flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
}
.h-top{
  width: 100%;
  height: 250rpx;
  background: -webkit-linear-gradient(left, rgb(255,132,0) , rgb(255,84,0));
}
.p-title{
  margin-left: 20rpx;
  color: #fff;
  font-size:28rpx; 
  margin-bottom:20rpx;
  margin-top:20rpx;

}
.p-box{
  position: relative;
  width: 100%;
  text-align: center;
  color: #fff;
  font-size: 60rpx;
  margin-bottom: 40rpx;
}
.p-box .dw{
  font-size: 24rpx;
} 
.hint-box{
  text-align: center;
  width: 100%;
  font-size: 24rpx;
  color: #fff;
}
.return-deposit{
  /*position: absolute;*/
  display: inline-block;
  width: 90rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  text-align: center;
  border-radius: 5rpx;
  background: #fff;
  margin-left: 10rpx;
  color: #ff5400;
}
.nav-wrap{
  display: -webkit-flex;
  width: 100%;
  height: 80rpx;
  border-bottom: 1px solid #eee;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.nav-li{
  width: 50%;
  text-align: center;
  font-size: 28rpx;
  line-height: 80rpx;
}
.active{
  border-bottom:2px solid #ff5400;
  color: #ff5400;
}
.tab-cont{
  flex: 1;
}
.scoll-h{
  position: relative;
  width: 100%;
  height: 100%;
}
.no-wrap{
  width: 100%;
  position: relative;
  top: 200rpx;
}
.no-data-img{
  margin:0 auto;
    width: 283rpx;
    height: 287rpx;
}
.no-txt{
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  color: #999;
}
.ticket-box{
  padding-left: 20rpx;
  box-sizing: border-box;
}
.ticket-li{
  display: -webkit-flex;
  width: 100%;
  padding: 20rpx 0;
  border-bottom: 1px solid #eee;
}
.tik-pic{
  width: 72rpx;
  height: 72rpx;
  margin-right: 20rpx;
}
.tik-pic image{
  width: 100%;

}
.tik-cont{
  flex: 1;
}
.tik-title{
  width: 100%;
  font-size: 32rpx;
  margin-bottom: 10rpx;
}
.tik-title text{
  float: right;
  margin-right: 20rpx;
}
.tik-time{
  font-size: 24rpx;
  color: #999;
}
.reward-btn{
  float: right;
  font-size: 26rpx;
  margin-right: 20rpx;
}
.reward-btn-blue{
  color: #4DAAF0;
}
.reward-btn-gray{
  color: #CCCCCC;
}
.reward-btn-error{
  color: #FF5400;
}
.reward-btn-active::after{
  content: '';
  display: inline-block;
  width: 14rpx;
  height: 24rpx;
  vertical-align: -2rpx;
  margin-left: 8rpx;
  background: url(http://uuweb.haofang.net/Public/wxApp/images/registration/arrow_right.png) center no-repeat;
  background-size: 100% 100%;
}
/*优惠券*/
.coupon-box-has{
  width: 681rpx;
  height: 259rpx;
  margin: 0 auto;
  color: #999;
  margin-bottom: 20rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/myAccount/yhj_gq.png) no-repeat center;
  background-size: 100%;
}
.coupon-box{
  width: 681rpx;
  height: 259rpx;
  margin: 0 auto;
  color: #999;
  margin-bottom: 20rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/myAccount/yhj_lv.png) no-repeat center;
  background-size: 100%;
}
.cou-title{
  padding: 20rpx 20rpx;
  width: 100%;
  font-size: 32rpx;
  box-sizing: border-box;
}
.no-use{
  color: #00ca85!important;
}
.cou-title text{
  float: right;
}
.from-box{
  width: 100%;
  padding: 0 20rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  margin-bottom:40rpx; 
}
.from-box text{
  
  float: right;
  font-size:32rpx; 
}
.cou-msg{
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  word-wrap: normal;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.red-packet{
  padding-top: 30rpx;
  background: #f6f6f6;
  box-sizing: border-box;
}

/** 优惠券:新版样式 **/
.coupan-li{
  position: relative;
  width: 690rpx;
  height: 210rpx;
  margin: 0 auto;
  display: -webkit-box;
  -webkit-box-align: center;
  background: url(http://uuweb.haofang.net/Public/wxApp/images/myAccount/coupan_li_bg.png) center center no-repeat;
  background-size: 100% 100%;
  margin-bottom: 20rpx;
}
.coupan-li .coupan-info{
  -webkit-box-flex: 1;
  line-height: 1;
  padding-left: 30rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.coupan-li .coupan-info .title{
  font-size: 34rpx;
  color: #333;
  margin-bottom: 22rpx;
}
.coupan-li .coupan-info .date{
  font-size: 24rpx;
  color: #bbb;
  margin-bottom: 22rpx;
}
.coupan-li .coupan-info .desc{
  font-size: 24rpx;
  color: #bbb;
}
.coupan-li .coupan-num{
  width: 250rpx;
  color: #fe8019;
  text-align: center;
  line-height: 1;
}
.coupan-li .coupan-num .num-text{
  font-size: 34rpx;
}
.coupan-li .coupan-num .num-text text{
  font-size: 54rpx;
  margin-left: 0rpx;
}
.coupan-li .coupan-num .num-desc{
  font-size: 22rpx;
  margin-top: 20rpx;
  letter-spacing: 1rpx;
}

.coupan-li-disabled .coupan-info .title,
.coupan-li-disabled .coupan-info .date,
.coupan-li-disabled .coupan-info .desc,
.coupan-li-disabled .coupan-num
{
  color: #CCCCCC;
}

/** 优惠券:新版样式 :end **/