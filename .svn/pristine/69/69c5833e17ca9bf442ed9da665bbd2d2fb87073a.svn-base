@keyframes vrRotate
{
  0% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/1.png);}
  8% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/2.png);}
  16% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/3.png);}
  32% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/4.png);}
  40% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/5.png);}
  48% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/6.png);}
  56% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/7.png);}
  64% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/8.png);}
  72% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/9.png);}
  80% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/10.png);}
  88% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/11.png);}
  96% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/12.png);}
  100% {background-image: url(http://uuweb.haofang.net/Public/wxApp/images/detail/720/13.png);}
}


@keyframes vdRotate
{
  0% {background-image: url(http://cdn.haofang.net/static/uuminiapp/detail/vdicon/1.png);}
  16% {background-image: url(http://cdn.haofang.net/static/uuminiapp/detail/vdicon/2.png);}
