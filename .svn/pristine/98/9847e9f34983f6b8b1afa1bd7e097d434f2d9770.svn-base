  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/entrustDetail/ic_unselect_fake_source.png);
}
.true-house-box .real-house-btn{
  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/entrustDetail/ic_unselect_real_source.png);
}
.true-house-box .unreal-house-btn.select{
  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/entrustDetail/ic_select_fake_source.png);
}
.true-house-box .real-house-btn.select{
  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/entrustDetail/ic_select_real_source.png);
}
.unreal-house-text{
  text-align:center;
font-size:26rpx;
color:#333;
width:530rpx;
margin:0 auto 65rpx;
line-height:40rpx;
}

.see-evaluate-box .star_box{
  width:548rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  margin-bottom: 35rpx;
  color:#999;
  font-size: 33rpx;
  height:50rpx;
  line-height:50rpx;
}
.see-evaluate-box .star_box view{
  background-size: 100% 100%;
  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/entrustDetail/ic_star_gray.png);
  background-repeat: no-repeat;
}
.see-evaluate-box .star_box .activestar{
  background-image:url(https://uuweb.haofang.net/Public/wxApp/images/entrustDetail/ic_star_yellow.png);
}
.see-evaluate-box .star_box view{
  width:50rpx;
  height: 50rpx;
}
.see-evaluate-box .reason_box_remark{
  position: relative;
  width: 606rpx;
  height: 166rpx;
  overflow: hidden;
  margin: 0 auto 38rpx;
}
.see-evaluate-box .reason_box_remark textarea{
  background-color: #f1f1f1;
  width: 100%;
  height: 100%;
  font-size: 28rpx;
  border-radius: 8rpx;
  padding: 15rpx 20rpx;
  box-sizing: border-box;
  color: #000;
}
.see-evaluate-box .reason_box_remark .reason-remark-text-placeholder{
  font-size: 28rpx;
  color: #888;
  background-color: #f1f1f1;
}
.see-evaluate-box .reason_box_remark view{
  font-size: 26rpx;
  position: absolute;
  right: 15rpx;
  bottom: 18rpx;
  color: #888;
}
.see-evaluate-box .reason_box{
  width: 606rpx;
  margin: 0 auto;
  display: flex;
  flex-wrap:wrap;
  overflow: hidden;
  margin-bottom: 70rpx;
  margin-top:40rpx;
}
.see-evaluate-box .reason_box .reason{
  width:280rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  text-align: center;
  border: 1rpx solid #ddd;
  border-radius: 4rpx;
  color:#999;
  position: relative;
  box-sizing: border-box;
}
 .see-evaluate-box .reason_box .reason:nth-child(1),
.see-evaluate-box .reason_box .reason:nth-child(3){
  margin-right: 45rpx;
}
.see-evaluate-box .reason_box .reason:first-child{
  margin-bottom: 26rpx;
} 

.see-evaluate-box .reason_box .reason.select{
  border: 1rpx solid #333;
  color: #333;
  background-color: #f6f6f6;
}
/* 带看评价弹框end */