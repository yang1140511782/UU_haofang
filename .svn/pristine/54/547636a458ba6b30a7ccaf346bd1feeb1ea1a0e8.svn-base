}
.zhiye-contact-voice::before{
    background-image: url(https://static.haofang.net/Public/images/wap/zhiye_voice.png);
    background-size: 52rpx 70rpx;
}
.zhiye-contact-call::before{
    background-image: url(https://static.haofang.net/Public/images/wap/zhiye_call.png);
    background-size: 54rpx 60rpx;
}
.queryMask{
  position: fixed;
  width:100%;
  height: 100%;
  top:0;
  left:0;
  background: rgba(0,0,0,0.5);
  z-index: 100;
}
.queryMask .queryBox{
  background: #fff;
  height: 672rpx;
  position: absolute;
  width:100%;
  bottom:0;
  left:0;
}
.queryMask .queryBox .h3{
  height: 103rpx;
  line-height: 103rpx;
  font-size: 30rpx;
  color:#404040;
  padding-left: 43rpx;
  letter-spacing: 1px;
  border-bottom: 1rpx solid #d6d6d6;
}
.queryMask .queryBox .close{
  position: absolute;
  top:0;
  right:0;
  width:103rpx;
  height: 103rpx;
  line-height: 103rpx;
  text-align: center;
  color:#5d5d5d;
  font-size: 40rpx;
}
.queryMask .queryBox .archivebox{
  height:412rpx;
  padding-top:38rpx;
  box-sizing: border-box;
}
.queryMask .queryBox .archivebox .avatar{
  width:154rpx;
  height: 154rpx;
  overflow: hidden;
  border-radius: 77rpx;
  margin: 0 auto;
  background: url(https://static.haofang.net/Public/images/call_header.png) no-repeat center center;
  background-size: cover;
}
.queryMask .queryBox .archivebox .avatar image{
  width:100%;
  height: 100%;
}
.queryMask .queryBox .archivebox .name{
  color:#000000;
  letter-spacing: 1px;
  font-size: 28rpx;
  line-height: 1;
  padding-top: 42rpx;
  padding-bottom: 10rpx;

  text-align: center;
}
.queryMask .queryBox .archivebox .star{
  width:100%;
  height: 38rpx;
  display: flex;
  align-items:center;
  justify-content: center;
}
.queryMask .queryBox .archivebox .star .starbox{
  width:120rpx;
  height: 23rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/changeImg/star_icon.png) no-repeat center center;
  background-size: 100%;
}
.queryMask .queryBox .archivebox .star .score{
  background: #feae00;
  color:#fff;
  font-size: 20rpx;
  line-height: 1;
  padding:7rpx 10rpx;
  border-radius: 8rpx;
  margin-left: 20rpx;
}
.queryMask .queryBox .archivebox .welcome{
  width:515rpx;
  text-align: center;
  color:#606060;
  line-height: 34rpx;
  margin: 10rpx auto;
  font-size: 24rpx;
}
.swiperbox{
  height: 154rpx;
}
.download{
  width:490rpx;
  height: 645rpx;
  z-index: 500;
  position: absolute;
  left: 0;
  right:0;
  margin: auto;
  top:13%;
}
.download image{
  width:100%;
  height: 100%;
}
.downMask{
  position: fixed;
  width:100%;
  height: 100%;
  top:0;
  left:0;
  background: rgba(0,0,0,0.5);
  z-index: 500;
}
.closeBtn{
  width:90rpx;
  height: 90rpx;
  background: url(https://static.haofang.net/Public/images/wap/list-bomp-close.png) no-repeat center center;
  background-size: 100%;
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  bottom: -130rpx;
}
.clooect-toast{
  position: fixed;
  top: 650rpx;
  left: 50%;
  transform: translateX(-50%);
  background: rgba(0,0,0,0.5);
  padding: 20rpx 15rpx;
  color: #fff;
  font-size: 26rpx;
  z-index: 1000;
  border-radius: 5rpx;
}
.gray{
 color: #ccc!important;
}
.down-app-box{
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
}
.down-mask{
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  width: 100%;
  background: rgba(0, 0, 0, 0.3);
  height: 100%;
}
.down-con{
  position: absolute;
  top: 50%;
  left: 50%;
  width: 600rpx;
  transform: translate(-50%,-50%);
  -webkit-transform: translate(-50%,-50%);
  -moz-transform: translate(-50%,-50%);
  -ms-transform: translate(-50%,-50%);
  -o-transform: translate(-50%,-50%);
  z-index: 20;
  box-sizing: border-box;
  border-radius: 5px;
  background: #fff;
}
.down-txt{
  font-size: 26rpx;
  text-align: center;
  font-weight: bold;
}
.down-txt-top{
  font-size: 30rpx;
  padding-top:30rpx; 
  margin-bottom: 10rpx;
}
.down-txt-bottom{
   font-weight: normal;
   color: #6b6f84;
}
.down-img{
  width: 427rpx;
  height: 321rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/mine/down-bg.png) center center no-repeat;
  background-size: 100% 100%;
  margin: 0 auto
}
.down-btn-txt{
  background: #01ca86;
  color: #fff;
  font-size: 32rpx;
  width: 490rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  margin: 40rpx auto;
  border-radius: 8rpx;
}
.d-top-img{
  width: 582rpx;
  height: 265rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/im/d-top.png) no-repeat center;
  background-size: 100%;
  margin: 10rpx auto;
}
#homeBtn{
  width:88rpx;
  height: 88rpx;
  position: fixed;
  right:21rpx;
  bottom:160rpx;
  border-radius: 50%;
  background: url('https://uuweb.haofang.net/Public/wxApp/images/personalCenter/index-btn.png') center center no-repeat;
  background-size: 100% 100%;
  z-index: 9;
}
/*底部边距  */
.bor-bot{
  border-bottom:10rpx solid #f6f6f6;
}