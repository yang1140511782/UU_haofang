/* toast */
.point-dialog{
    position: fixed;
    top:0;
    width: 100%;
    max-width: 640px;
    height: 100%;
    z-index: 999990;
}
.dialog-content{
    position: absolute;
    top:50%;
    left: 50%;
    width: 500rpx;
    background: #fff;
    border-radius: 5px;
    padding-bottom: 48rpx;
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
}
.dialog-toast{
    position: absolute;
    top:50%;
    left: 50%;
    border-radius: 8px;
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    background: rgba(0,0,0,0.7);
}
