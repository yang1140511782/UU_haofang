/*轮播大图*/

/*大图*/
.big-image-mask,.list-image-mask{
    position: fixed;
    top: 0;
    height: 100%;
    width: 100%;
    background: rgba(0,0,0,.4);
    z-index: 9999;
    max-width: 640px;
    margin: 0 auto;
}
.big-image-swiper,.list-image-swiper{
    height: 100%;
    width: 100%;
    overflow: hidden;
    position: relative;
}
.small-swiper-pic .swiper-lazy{
    background-size: cover;
    background-position: center center;
}
.big-image-swiper .swiper-slide,.list-image-mask .swiper-slide{
    width: 100%;
    height: 100%;
    background-color: #000;
    position: relative;
}
.big-image-swiper .swiper-slide image,.list-image-swiper .swiper-slide image{
    width: 100%;
    height: auto;
    display: block;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
}
.big-image-mask .swiper-slide-active{
    position: relative;
}
.big-image-mask .swiper-slide-active::before{
    content: '';
    display: inline-block;
    width: 100%;
    height: 400rpx;
    background: #FFFFFF url(https://uuweb.haofang.net/Public/images/newhouse/images/swiper-bg.gif) center no-repeat;
    background-size: 50% auto;
    position:absolute;
    left: 50%;
    top:50%;
    transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
}
