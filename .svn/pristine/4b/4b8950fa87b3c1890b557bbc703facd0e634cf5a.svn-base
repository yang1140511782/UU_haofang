}
.toolbar{
  position: relative;
  background: #eee;
  height:50px;
  width: 100%;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  font-size: 28rpx;
  margin: 0;
  z-index: 500;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
}
.toolbar-middle{
  position: absolute;
