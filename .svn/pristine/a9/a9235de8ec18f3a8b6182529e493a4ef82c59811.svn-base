  width: 150rpx;
  height: 30rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/detail/score_yellow.png) no-repeat;
  background-size: 90% auto;
  vertical-align: middle;
  background-position:0 0rpx;
}
.dw-btn{
  position: absolute;
  top:0;
  bottom: 0;
  margin: auto; 
  right: 20rpx;
  width: 80rpx;
  height: 50rpx;
  border: 1px solid #0d48ab;
  color: #0d48ab;
  font-size: 30rpx;
  line-height: 50rpx;
  border-radius: 5rpx;
  text-align: center;
}
/*预约看房弹框*/
.order-toast{
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  top: 150rpx;
  width: 600rpx;
  border-radius:8rpx; 
  padding-top: 40rpx;
  box-sizing: border-box;
  z-index: 1200;
}
.order-txt{
  width: 100%;
  text-align: center;
  font-size:32rpx; 
  margin-bottom: 40rpx;
}
.sub-btn{
  position: absolute;
  left: 0;
  bottom: 0;
  border-radius:0 0 8rpx 8rpx;  
  width: 100%;
  height: 80rpx;
  background: #00ca85;
  color: #fff;
  font-size: 32rpx;
  line-height: 80rpx;
  text-align: center;
}
.info-box{
  width: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.input-box{
  width: 100%;
  height: 60rpx;
  background: red;
  border-bottom: 1px solid #eee;
}
.contract-box{
  display: -webkit-flex;
  width: 100%;
  height: 150rpx;
  font-size: 20rpx;
  text-align: center;
  justify-content: space-around;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.chat-b,.call-b{
  position: relative;
  width: 30%;
}
.chat-ic,.call-ic{
  position: relative;
  left: 0;
  right: 0;
  margin: auto;
  width: 70rpx;
  height: 70rpx;
  margin-bottom: 30rpx;
}
.chat-ic{
  background: url(https://uuweb.haofang.net/Public/wxApp/images/detail/chat_pic.png) no-repeat;
  background-size: 100%;
}
.call-ic{
  background: url(https://uuweb.haofang.net/Public/wxApp/images/detail/tel_pic.png) no-repeat;
  background-size: 100%;
}

/*引导下载*/
.lead-toast{
  position: absolute;
  right: 0;
  bottom: 160rpx;
  width: 300rpx;
  height: 200rpx;

}
.log-pic{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 64rpx;
  height: 39rpx;
}
.lt-txt{
  width: 100%;
  font-size: 26rpx;
  text-align: center;
  color: #fff;
}
.lead-pic{
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  width: 94rpx;
  height: 82rpx;
}
.order-icon{
  width: 490rpx;
  height: 645rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/apartmentDetail/detail-down-img.png)no-repeat center;
  background-size: 100%;
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
}
.order-close{
  width: 90rpx;
  height: 90rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/apartmentDetail/list-bomp-close.png)no-repeat center;
  background-size: 100%;
