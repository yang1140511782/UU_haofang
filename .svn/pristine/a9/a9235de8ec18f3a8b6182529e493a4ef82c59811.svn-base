  left: 0;
  right: 0;
  margin: auto;
  top: 740rpx;
}
.nav{
    background-color:#fff;
    font-size:24rpx;
    width:100%;
    overflow-x: auto;
    overflow-y: hidden;
    padding-top:20rpx;
    padding-bottom:20rpx;
    box-sizing: border-box;
}
.nav-but{
  border: cadetblue 1px solid;
  border-radius: 0px;
  width: 25%;
  text-align: center;
  line-height: 30px;
  display: inline-flex;
}
#scroll-map{
    width: 120%;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-align: center;
}
::-webkit-scrollbar{width:0px;height: 0}
.scroll-view {
	width: 100rpx;
	height: 100%;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.common{
	width: 50rpx;
	height: 50rpx;
	background-position: center center;
    background-repeat: no-repeat;
    background-size: 100% 100%;
}
.map-type{
	width:120rpx;
    height:50rpx;
    line-height:50rpx;
    font-size:24rpx;
    text-align:center;
}
.bus-icon{  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/map/transport.png);  }
.subway-icon{  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/map/subway.png);  }
.school-icon{  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/map/school.png);  }
.hospital-icon{  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/map/hospital.png);  }
.bank-icon{  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/map/bank.png);  }
.leisure-icon{  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/map/leisure.png);  }
.shopping-icon{  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/map/shopping.png);  }
.food-icon{  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/map/food.png);  }
.sports-icon{  background-image: url(https://uuweb.haofang.net/Public/wxApp/images/map/sports.png);  }
.down-con{
  position:absolute;
  top:50%;
  left:50%;
  width:640rpx;
  transform:translate(-50%,5%);
  transform:translate(-50%,5%);
  z-index:20;
  box-sizing:border-box;
  border-radius:5px;
  background:#fff;
}
.down-txt{
  font-size: 30rpx;
  text-align: center;
  font-weight: bold
}
.down-txt-top{
  padding-top:55rpx;
  font-weight: normal
}
.down-txt-bottom{
   padding-bottom:40rpx
}
.down-img{
  width: 427rpx;
  height: 321rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/mine/down-bg.png) center center no-repeat;
  background-size: 100% 100%;
  margin: 0 auto
}
.down-btn-txt{
  background: #17b980;
  color: #fff;
  font-size: 34rpx;
  width: 523rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin: 40rpx auto;
  border-radius: 4rpx;
}
