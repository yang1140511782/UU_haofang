.filter{
  height: 80rpx;
  border-bottom: 1px solid #f6f6f6;
  background: #ffffff;
  display: flex;
}
.filter .itembtn{
  flex:1;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  color: #494949;
  position: relative;
}
.filter .itembtn::after{
    content: "";
    width:15rpx;
    height: 9rpx;
    background: url(http://cdn.haofang.net/static/uuminiapp/new_images/down.png) no-repeat 0 0;
    background-size: 100%;
    display: inline-block;
    margin-left: 15rpx;
    vertical-align: middle;
}
.filter .itembtn:not(:last-child)::before{
  content:'';
  position: absolute;
  width:1px;
  height: 30rpx;
  background: #d1d1d1;
  right:0;
  top:0;
  bottom:0;
  margin: auto;
}
.filter .choosing{
  color: #ff5400;
}
.filter .choosing::after{
  background-image: url(http://cdn.haofang.net/static/uuminiapp/new_images/down_ac_icon.png?t=20180427);
}
.filter .choosed{
  color: #ff5400;
}

