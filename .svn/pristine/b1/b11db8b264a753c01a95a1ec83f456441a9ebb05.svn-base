}

.sale-rent-houseItem .sale-rent-infobox .sale-rent-tag text {
  font-size: 22rpx;
  letter-spacing: 1px;
  display: inline-block;
  line-height: 1;
  color:#1bc964;
  background: #ddf7e8;
  padding:4rpx 6rpx;
  padding-top: 7rpx;
  border-radius: 3rpx;
  margin-right:10rpx;
}
.sale-rent-houseItem .sale-rent-infobox .sale-rent-tag text.true_flag{
  position: relative;
  top: 5rpx;
  width:81rpx;
padding-top:21rpx;
background:url(http://uuweb.haofang.net/Public/wxApp/images/index/true_house_flag.png) center bottom no-repeat;
background-size:81rpx 21rpx;
height:38rpx;
line-height:19rpx;
padding:0;
}

.sale-rent-houseItem .sale-rent-infobox .sale-rent-tag text:nth-child(4n+2) {
  color: #00a0e9;
  background: #d9f1fc;
}

.sale-rent-houseItem .sale-rent-infobox .sale-rent-tag text:nth-child(4n+3) {
  color: #ff9600;
  background: #ffeed8;
}

.sale-rent-houseItem .sale-rent-infobox .sale-rent-tag text:nth-child(4n+4) {
  color: #f46f66;
  background: #fdeae9;
}

.sale-rent-houseItem .sale-rent-infobox .sale-rent-price {
  font-size: 27rpx;
  color: #ff5a1f;
  font-weight: bold;
  line-height: 61rpx;
}

.sale-rent-houseItem .sale-rent-infobox .sale-rent-price text {
  color: #999;
  font-weight: normal;
  font-size: 22rpx;
  margin-left: 18rpx;
}

.sale-rent-houseItem .sale-rent-infobox .sale-rent-top {
  font-size: 16rpx;
  letter-spacing: 1px;
  color: #a5a5a5;
  padding: 6rpx;
  border: 1px solid #dcdcdc;
  border-radius: 3rpx;
  position: absolute;
  line-height: 1;
  right: 0;
  bottom: 12rpx;
}

.sale-rent-tbb {
  position: absolute;
  width: 100%;
  height: 34rpx;
  padding-left: 8rpx;
  box-sizing: border-box;
  line-height: 34rpx;
  display: block;
  bottom: 12rpx;
}

.sale-rent-icon-vr {
  width: 40rpx;
  height: 33rpx;
  display: inline-block;
  float: left;
  margin-right: 10rpx;
  background: url(https://youjia.haofang.net/Public/images/vr_x.png) no-repeat;
  background-size: 100%;
}

.sale-rent-icon-video {
  width: 40rpx;
  height: 33rpx;
  display: inline-block;
  background: url(https://youjia.haofang.net/Public/images/video_x.png) no-repeat;
  background-size: 100%;
}

