}

.apartment-houseItem .apartment-infobox .apartment-tag text:nth-child(4n+3) {
  color: #ff9600;
  background: #ffeed8;
}

.apartment-houseItem .apartment-infobox .apartment-tag text:nth-child(4n+4) {
