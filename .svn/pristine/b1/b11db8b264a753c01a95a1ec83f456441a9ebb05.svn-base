/* pages/collect/collect.wxss */

.head {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  border-bottom: 1rpx solid #ddd;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;
  background: #fff;
}

.head-item {
  width: 20%;
  height: 76rpx;
  font-size: 28rpx;
  text-align: center;
  box-sizing: border-box;
}

.head-item-current {
  color: #ff5400;
  border-bottom: 4rpx solid #ff5400;
  width: auto;
  height: 80rpx;
  box-sizing: border-box;
  display: inline-block;
}

/*新房*/

.newhouse-houseItem {
  display: flex;
  position: relative;
  margin-left: 21rpx;
  border-bottom: 1px solid #ddd;
  padding-bottom: 35rpx;
  background: #fff;
}

.newhouse-houseItem .newhouse-picbox {
  margin-top: 42rpx;
  width: 220rpx;
  height: 167rpx;
  position: relative;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/default/list_deafault.png) no-repeat center center;
  background-size: cover;
}

.newhouse-houseItem .newhouse-picbox image {
  width: 100%;
  height: 100%;
}

.newhouse-houseItem .newhouse-infobox {
  padding-left: 20rpx;
  padding-top: 36rpx;
  width: 445rpx;
  position: relative;
}

.newhouse-houseItem .newhouse-infobox .newhouse-housetitle {
  font-size: 28rpx;
  color: #000;
  line-height: 38rpx;
  margin-bottom: 8rpx;
  letter-spacing: 1px;
  font-family: '黑体';
  overflow: hidden;
}

.newhouse-houseItem .newhouse-infobox .newhouse-housetitle text {
  font-family: '微软雅黑';
  line-height: 1;
  font-size: 24rpx;
  margin-left: 12rpx;
  padding: 5rpx;
  padding-bottom: 7rpx;
  display: inline-block;
  color: #fff;
}

.newhouse-houseItem .newhouse-infobox .newhouse-housetitle .newhouse-wait {
  background: #14c4f9;
}

.newhouse-houseItem .newhouse-infobox .newhouse-housetitle .newhouse-selling {
  background: #0ac95e;
}

.newhouse-houseItem .newhouse-infobox .newhouse-housetitle .newhouse-soldOut {
  background: #ccc;
}

.newhouse-houseItem .newhouse-infobox .newhouse-text {
  font-size: 24rpx;
  line-height: 1;
  color: #8c8c8c;
  padding-top: 11rpx;
  padding-bottom: 11rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.newhouse-houseItem .newhouse-infobox .newhouse-tag {
  line-height: 1;
}

.newhouse-houseItem .newhouse-infobox .newhouse-tag text {
  font-size:22rpx;
  letter-spacing:1px;
  display:inline-block;
  color:#1bc964;
  background:#e0f7ea;
  padding:6rpx 8rpx;
  padding-top:8rpx;
  border-radius:3rpx;
  margin-right:10rpx;
  line-height:1;

}

.newhouse-houseItem .newhouse-infobox .newhouse-tag text:nth-child(4n+2) {
  color: #00a0e9;
  background: #d9f1fc;
}

.newhouse-houseItem .newhouse-infobox .newhouse-tag text:nth-child(4n+3) {
  color: #ff9600;
  background: #ffeed8;
}

.newhouse-houseItem .newhouse-infobox .newhouse-tag text:nth-child(4n+4) {
  color: #f46f66;
  background: #fdeae9;
}

.newhouse-houseItem .newhouse-infobox .newhouse-price {
  font-size: 27rpx;
  color: #ff5a1f;
  line-height: 61rpx;
}

.newhouse-houseItem .newhouse-infobox .newhouse-price text {
  color: #999;
  font-weight: normal;
  font-size: 22rpx;
  margin-left: 18rpx;
}

.newhouse-houseItem .newhouse-infobox .newhouse-top {
  font-size: 16rpx;
  letter-spacing: 1px;
  color: #a5a5a5;
  padding: 6rpx;
  border: 1px solid #dcdcdc;
  border-radius: 3rpx;
  position: absolute;
  line-height: 1;
  right: 0;
  bottom: 12rpx;
}

/*出租出售样式*/

.content {
  height: 100%;
  box-sizing: border-box;
  padding-top: 90rpx;
}

.sale-rent-houseItem {
  display: flex;
  margin-left: 30rpx;
  border-bottom: 1px solid #ebebeb;
  padding-bottom: 29rpx;
  position: relative;
}

.sale-rent-houseItem .sale-rent-picbox {
  margin-top: 42rpx;
  width: 210rpx;
  height: 157rpx;
  position: relative;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/default/list_deafault.png) no-repeat center center;
  background-size: cover;
}

.sale-rent-houseItem .sale-rent-picbox image {
  width: 210rpx;
  height: 157rpx;
}

.sale-rent-houseItem .sale-rent-infobox {
  padding-left: 33rpx;
  padding-top: 36rpx;
  width: 445rpx;
  position: relative;
}

.sale-rent-houseItem .sale-rent-infobox .sale-rent-housetitle {
  font-size: 28rpx;
  color: #333;
  line-height: 38rpx;
  max-height: 83rpx;
  margin-bottom: 5rpx;
  letter-spacing: 1px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.sale-rent-houseItem .sale-rent-infobox .sale-rent-text {
  font-size: 24rpx;
  line-height: 1;
  color: #888;
  padding-bottom: 15rpx;
}

.sale-rent-houseItem .sale-rent-infobox .sale-rent-tag {
