/*屏蔽提示框*/
.toast-tip{
    position: absolute;
    bottom: 0;
    left: 0;
    z-index: 200;
}
.toast-tip-tt{
    position: fixed;
    /*width: 230rpx;*/
    padding: 0 20rpx;
    height: 70rpx;
    line-height: 70rpx;
    left: 50%;
    top: 50%;
    margin-top: -70rpx;
    -webkit-transform: translate(-50%,0);
    -moz-transform: translate(-50%,0);
    -ms-transform: translate(-50%,0);
    -o-transform: translate(-50%,0);
    transform: translate(-50%,0);
    text-align: center;
    font-size: 24rpx;
    background-color: rgba(0,0,0,0.7);
    color: #fff;
    border-radius: 8rpx;
}
