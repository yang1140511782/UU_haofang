@import "../../component/loadDown/loadDown.wxss";
page {
  height: 100%;
  background-color: #f2f2f2;
  width: 100%;
  margin: 0;
  font-family: "微软雅黑", "Helvetica Neue", Helvetica, STHeiTi, sans-serif;
}
.page{
  height: 100%;
}
/*im界面头部*/

.im-header {
  width: 100%;
  height: 50px;
  position: fixed;
  top: 0;
  display: flex;
  flex-direction: row;
  z-index: 1000;
  padding: 0 10rpx;
  align-items: center;
  background-color: #fff;
  border-bottom: 1px solid #fff;
}

.im-header .im-header-face {
  width: 47px;
  height: 47px;
  border-radius: 50%;
  overflow: hidden;
}

.im-header-face image {
  width: 47px;
  display: inline;
  border-radius: 100%;
}

.im-header .im-header-info {
  width: 75%;
  height: 85%;
  line-height: 1.2;
  display: flex;
  align-items: flex-start;
  flex-direction: column;
  justify-content: center;
  padding-left: 10rpx;
  color: #00ca85;
}

.im-header-info .im-username {
  height: 50%;
  line-height: 22px;
  font-size: 15px;
  overflow: hidden;
  display: flex;
  align-items: center;
}

.im-header-info .im-usermobile {
  height: 50%;
  font-size: 14px;
  display: flex;
  align-items: center;
}

/*im内容显示界面*/

.im-container {
  width: 100%;
  height: 100%;
  color: #333;
  overflow: auto;
  position: relative;
}
.scrollBox{
  padding:20rpx;
  padding-bottom: 103rpx;
  box-sizing: border-box;
}

.im-container .me {
  width: 100%;
  display: flex;
  flex-direct: row-reverse;
  justify-content: flex-end;
  margin-bottom: 25rpx;
  padding-right: 20rpx;
}

.im-container .other {
  width: 100%;
  display: flex;
  flex-direct: row-reverse;
  justify-content: flex-start;
  margin-bottom: 10px;
}

.me .me-face, .other .other-face {
  width: 40px;
  height: 40px;
  overflow: hidden;
}

.me-face image, .other-face image {
  width: 40px;
  display: inline;
}

.me .me-content {
  background-color: #4DAAF0;
  color: #fff;
  word-break: break-all;
  line-height: 1.5;
  position:relative;
  margin-right:22rpx;
  min-height:40px;
  padding:19rpx 20rpx;
  box-sizing: border-box;
  max-width:65%;
  border-radius:15rpx;
  text-align:left;
  font-size:15px;
}

.me .me-content::after{
  content: '';
  position: absolute !important;
  top: 15rpx;
  width: 20rpx;
  height: 20rpx;
  right: -17rpx;
  background: url('https://uuweb.haofang.net/Public/wxApp/images/im/right_icon.png') no-repeat;
  background-size: 17rpx 13rpx;
}
.me .house-box::after{
  content: '';
  position: absolute !important;
  top: 30rpx;
  width: 20rpx;
  height: 20rpx;
  right: -17rpx;
  background: url('https://uuweb.haofang.net/Public/wxApp/images/im/right_white_icon.png') no-repeat;
  background-size: 17rpx 13rpx;
}
.me .image::after{
  content: '';
  position: absolute !important;
  top: 15rpx;
  width: 20rpx;
  height: 20rpx;
  right: -17rpx;
  background: none !important;
}
.house-box{
  background-color: #fff;
  color: #4E5647;
  word-wrap: break-word;
  position: relative;
  margin-right: 22rpx;
  min-height: 30px;
  padding: 20rpx;
  max-width: 65%;
  border-radius: 20rpx;
  text-align: left;
  font-size: 15px;
}
.house-chat-title{
  margin-bottom:20rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  font-size:30rpx;
  color:#424242;
  font-weight:800;
}
.other .padding0 image {
  border-top-right-radius:20rpx;
  border-bottom-left-radius:20rpx;
  border-bottom-right-radius:20rpx;
}

.other .other-content {
  background-color: #fff;
  color: #041c0c;
  line-height: 1.5;
  position: relative;
  margin-left: 22rpx;
  min-height: 30px;
  padding:19rpx 20rpx;
  box-sizing: border-box;
  max-width: 65%;
  border-radius: 15rpx;
  text-align: left;
  word-break:break-all;
  font-size: 15px;
}
.other .other-content::before{
  content: '';
  position: absolute !important;
  top: 15rpx;
  width: 20rpx;
  height: 20rpx;
  left:-16rpx;
  border-right: 10rpx solid #fff;
  background: url('https://uuweb.haofang.net/Public/wxApp/images/im/left_icon.png') no-repeat;
  background-size: 17rpx 13rpx;
}
.other .image::before{
  content: '';
  position: absolute !important;
  top: 15rpx;
  width: 20rpx;
  height: 20rpx;
  left:-16rpx;
  border-right: 10rpx solid #fff;
  background: none !important;
}
#emoji {
  width: 25px!important;
  height: 25px!important;
}

.other-content image, .me-content image{
  width: 200px;
}

.other-content .emoji,.me-content .emoji {
  width: 46rpx;
  height: 46rpx;
  vertical-align:middle;
  line-height: 23px;
}

.me-corner-right {
  width: 15px;
  height: 14px;
  position: absolute;
  top: 50%;
  margin-top: -7px;
  right: -15px;
  background: url(https://weidian.haofang.net/Public/web/default/images/word_cornor_left4.png?SSL=1) center center no-repeat;
  background-size: 100% auto;
}

.other-corner-left {
  width: 15px;
  height: 15px;
  position: absolute;
  top: 50%;
  margin-top: -7px;
  left: -15px;
  background: url(https://weidian.haofang.net/Public/web/default/images/word_cornor_right.png?SSL=1) right center no-repeat;
  background-size: 25px 40px;
}

.im-time {
  width: 100%;
  height: 30px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.im-time text {
  height: 80%;
  font-size: 12px;
  background-color: #d1d1d1;
  color: #fff;
  border-radius: 10px;
  padding: 2px 8px;
  display: flex;
  justify-content: center;
  align-items: center;
}

/*im底部内容输入界面*/

.im-bottom {
  height: 100rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  border-top: 1px solid #efefef;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}
.toTop{
  bottom:250rpx!important;
}

.im-bottom .audio-icon{
  height: 80rpx;
  width: 80rpx;
  text-align: center;
  line-height: 74rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.im-bottom .audio-icon image{
  height: 65rpx;
  width: 65rpx;
  vertical-align: middle;
}
.im-bottom .im-input {
  width: 70%;
  height: 75%;
  border-bottom: 1px solid #e6e6e6;
}
.im-bottom .im-voice {
  width: 70%;
  height: 75%;
  border: 1px solid #e6e6e6;
  border-radius: 5px;
  text-align:center;
  vertical-align:middle;
  color:#666;
  line-height: 75rpx;
}

.im-bottom .im-input input {
  width: 100%;
  height: 120%;
  display: flex;
  justify-content: center;
  padding-left: 10rpx;
  padding-right: 15rpx;
  box-sizing: border-box;
}

.im-bottom .im-input-placeholder {
  color: #999;
  height: 100%;
  line-height: 100%;
  font-size: 14px;
  font-family: "微软雅黑", "Helvetica Neue", Helvetica, STHeiTi, sans-serif;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.im-bottom .im-btn {
  width:21%;
  height:100%;
  border-radius:5px;
  margin-left:12rpx;
  margin-right:17rpx;
  position:relative;
  z-index:20000;
  display:flex;
  align-items:center;
  justify-content:space-between;
}

.im-btn #im-btn-hover {
  background-color: #38c072;
  color: #fff;
}

.im-btn button {
  width: 100%;
  height: 97%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #333;
  font-size: 14px;
}

