.toastmask{
  width:100%;
  height: 100%;
  background: rgba(0,0,0,.6);
  top: 0;
  left: 0;
  position: fixed;
  z-index:1111;
}
.toastmask .toastbox{
  width: 638rpx;
  height: 320rpx;
  background: #ffffff;
  position: fixed;
  top:0;
  left:0;
  right:0;
  bottom:0;
  margin: auto;
  border-radius: 10rpx;
  text-align: center;
}
.toastmask .toastbox .name{
  height: 100rpx;
  line-height: 100rpx;
  color:#333;
  font-size: 32rpx;
  letter-spacing: 1px;
}
.toastmask .toastbox .note{
  color:#000000;
  padding:0 38rpx;
  letter-spacing: 1rpx;
  font-size: 28rpx;
}
.toastmask .toastbox .note cover-view{
  line-height: 55rpx;
  text-align: center;
}
.toastmask .toastbox .btn{
  border-top:1rpx solid #d7d7d7;
  position: absolute;
  width:100%;
  height: 93rpx;
  line-height: 93rpx;
  color:#ff5400;
  font-size: 32rpx;
  letter-spacing: 1px;
  left:0;
  bottom: 0;
}

