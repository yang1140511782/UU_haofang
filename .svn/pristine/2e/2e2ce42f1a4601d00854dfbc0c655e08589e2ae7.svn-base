  /**
   * 页面的初始数据
   */
  data: {
    currentTab: 0,
  },
    /**
   * tab切换
   */
  swichNav: function (e) {
    var cur = e.target.dataset.current;
    if (this.data.currentTaB == cur) { return false; }
    else {
      this.setData({
        currentTab: cur
      })
    }
  },
  /**
   * swiper切换变换nav
   */
