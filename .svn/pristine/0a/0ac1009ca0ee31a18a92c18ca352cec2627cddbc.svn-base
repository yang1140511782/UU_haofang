  font-size: 28rpx;
  color: #888;
  background-color: #f1f1f1;
}
.refuse-broker-box .reason_box_remark view{
  font-size: 26rpx;
  position: absolute;
  right: 15rpx;
  bottom: 18rpx;
  color: #888;
}
.refuse-broker-box .reason_box{
  width: 624rpx;
  margin: 0 auto;
  display: flex;
  flex-wrap:wrap;
  overflow: hidden;
  margin-top:75rpx;
  margin-bottom: 32rpx;
}
.refuse-broker-box .reason_box .reason{
  width:270rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  box-sizing: border-box;
  text-align: center;
  border: 1rpx solid #ddd;
  border-radius: 4rpx;
  color:#999;
  position: relative;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}
.refuse-broker-box .reason_box .reason:nth-child(1),
.refuse-broker-box .reason_box .reason:nth-child(3),
.refuse-broker-box .reason_box .reason:nth-child(5){
  margin-right: 64rpx;
}
.refuse-broker-box .reason_box .reason.select{
  border: 1rpx solid #333;
  color: #333;
  background-color: #f6f6f6;
}
