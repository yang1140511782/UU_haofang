  bottom: 0;
  width: 100%;
  overflow: hidden;
  background: #fff;
}
.refuse-broker-header{
  position: relative;
  text-align: center;
  font-size: 36rpx;
  color: #000;
  height: 128rpx;
  line-height: 128rpx;
  border-bottom: 1rpx solid #eee;
}
.refuse-broker-tt{}
.refuse-broker-close{
  position: absolute;
  width: 26rpx;
  height: 26rpx;
  top: 50%;
  right: 38rpx;
  margin-top: -13rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/entrustDetail/close2.png) center no-repeat;
  background-size: 100% 100%;
}
.refuse-broker-footer-line{
  position: relative;
  background: #eee;
  width: 100%;
  height: 10rpx;
}
.refuse-broker-footer{
  text-align: center;
  font-size: 36rpx;
  color: #cccdcf;
  height: 125rpx;
  line-height: 125rpx;
}
.refuse-broker-footer.refuse-broker-submit{
  color: #f64a12;
}
.refuse-broker-main{}

.refuse-broker-box .reason_box_remark{
  position: relative;
  width: 630rpx;
  height: 114rpx;
  overflow: hidden;
  margin: 0 auto 38rpx;
}
