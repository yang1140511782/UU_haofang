image{width: 100%;}

page{
  position: relative;
  width: 100%;
  height: 100%;
  background: #fff;
  font-family: "Helvetica Neue", Helvetica, Arial, "PingFang SC", "Hiragino Sans GB", "Heiti SC", "Microsoft YaHei", "WenQuanYi Micro Hei", sans-serif;
}
::-webkit-scrollbar{width:0px;height: 0}
.reward-wrap{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.banner-box{
  width: 100%;
  height: 420rpx;
  background: url(https://uuweb.haofang.net/PublicC/images/publish/rewardInstruction-bg.png) center center no-repeat;
  background-size: auto 100%;
  margin-bottom: 20rpx;
}
.cont-box{
  width: 100%;
  box-sizing: border-box;
  font-size: 28rpx;
  padding: 0 110rpx;
  padding-bottom: 51rpx;
  position: relative;
}
.cont-box::before{
    content: '';
    display: inline-block;
    width: 17rpx;
    height: 17rpx;
    background: url(https://uuweb.haofang.net/PublicC/images/publish/rewardInstruction-start-icon.png) center center no-repeat;
    background-size: 100% 100%;
    position: absolute;
    top: 10rpx;
    left: 80rpx;
}
.txt-b{
  width: 100%;
  color: #333;
}
.txt-b-a{
  width: 100%;
  color: #999;
  font-size: 20rpx;
}
.foot-btn{
  position: relative;
  width: 100%;
}
.btn-box{
  width: 80%;
  margin: 0 auto;
  text-align: center;
  border-radius: 5px;
  font-size: 34rpx;
  color: #fff;
  padding: 20rpx 0;
}
.reward-btn {
    background: #ff9c16;
    margin-bottom: 30rpx;
}
.common-btn {
    color: #777;
    background: #ececec;
    margin-bottom: 30rpx;
}
.reward-btn text {
    position: relative;
}
.foot-btn text {
    letter-spacing: 1px;
}
.reward-btn text::before {
    content: '';
    display: inline-block;
    width: 44rpx;
    height: 37rpx;
    background: url(https://uuweb.haofang.net/PublicC/images/publish/rewardInstruction-shang-icon.png) center center no-repeat;
    background-size: 100% 100%;
    position: absolute;
    top: 2rpx;
    left: -50rpx;
}

/*已发布过委托弹窗*/
.confirm-box{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.6);
  left: 0;
  top: 0;
  z-index: 50;
}
.confirm-box .confirm-content{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 648rpx;
  height: 250rpx;
  border-radius: 8rpx;
  overflow: hidden;
  background: #fff;
}
.confirm-box .confirm-content .confirm-tt{
  width: 100%;
  height: 155rpx;
  line-height: 156rpx;
  text-align: center;
  font-size: 30rpx;
  color: #000;
}
.confirm-box .confirm-content .confirm-btn-box{
  width: 100%;
  height: 95rpx;
  display: -webkit-box;
}
.confirm-box .confirm-content .confirm-btn-box .confirm-btn{
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  -webkit-box-flex: 1;
  border-top: 1rpx solid #ddd;
  font-size: 35rpx;
}
.confirm-box .confirm-content .confirm-btn-box .confirm-btn:first-child{
  border-right: 1rpx solid #ddd;
  color: #888;
}
.confirm-box .confirm-content .confirm-btn-box .confirm-btn:last-child{
  color: #1ac964;
}
/*已发布过委托弹窗end*/
/*红包弹框*/
.red-toast{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.4);
}
.r-box{
  position: relative;
  left: 0;
  right: 0;
  margin: auto;
  top: 200rpx;
  width: 540rpx;
  height: 740rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/rewardInstruction/red_bag_before.png) center center no-repeat;
  background-size: 100% 100%;
  z-index: 100;
  padding-top: 40rpx;
  box-sizing: border-box;
}
.r-box-a{
  position: relative;
  left: 0;
  right: 0;
  margin: auto;
  top: 200rpx;
  width: 540rpx;
  height: 740rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/rewardInstruction/red_bag_after.png) center center no-repeat;
  background-size: 100% 100%;
  z-index: 100;
  padding-top: 40rpx;
  box-sizing: border-box;
}
.close-btn{
  position: absolute;
  top: 0;
  width: 96rpx;
  height: 96rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/rewardInstruction/red_close.png) 20rpx 20rpx no-repeat;
  background-size: 25% 25%;
}
.a-tx{
  margin: 0 auto;
  width: 110rpx;
  height: 110rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/rewardInstruction/red_bag_agentheader.png) center center no-repeat;
  background-size: 100% 100%;
  margin-bottom: 20rpx;
  overflow: hidden;
  border-radius: 100rpx;
}
.a-tx image{
  width: 100%;
  height: 100%;

}
.a-name{
  width: 100%;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  margin-bottom: 20rpx;
}
.a-msg{
  width: 100%;
  text-align: center;
  color: #ffa596;
  font-size: 28rpx;
}
.open-box{
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  top: 380rpx;
  width: 166rpx;
  height: 174rpx;
  background: url(https://uuweb.haofang.net/Public/wxApp/images/rewardInstruction/open_red_bag.png) center center no-repeat;
  background-size: 100% 100%; 
}
.a-con{
  position: absolute;
  bottom: 20rpx;
  width: 100%;
  text-align: center;
  color: #ffa596;
  font-size: 28rpx;
}
.red-money{
  width: 100%;
  text-align: center;
  color: #fff;
  margin-top: 20rpx;
  font-size: 40rpx;
  margin-bottom: 20rpx;
}
.af-msg{
  width: 100%;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  margin-bottom: 40rpx;
}
.af-txt{
  width: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
  color: #ffa596;
  font-size: 22rpx;
}
.af-btn{
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 20rpx;
  width: 90%;
  height: 80rpx;
  background: #d04238;
  border-radius: 5rpx;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
}