/*** 房源列表样式 ***/
.houseItem{
  display: flex;
  margin-left: 26rpx;
  padding-right: 26rpx;
  border-bottom: 2rpx solid #ebebeb;
  padding-bottom: 20rpx;
  justify-content: space-between;
}
.houseItem:last-child {
  border-bottom: 0;
}
.houseItem .picbox{
  margin-top:37rpx;
  width:220rpx;
  height: 170rpx;
  position:relative;
  background: #f8f8f8 url(http://cdn.haofang.net/static/uuminiapp/default/list_deafault.png?t=20180606) no-repeat center center;
  background-size: cover;
}

.houseItem .picbox .bgImg{
  width:100%;
  height: 100%;
}
.houseItem .picbox .vri{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 60rpx;
  height: 60rpx;
}

.houseItem .infobox{
  flex: 1;
  padding-left: 33rpx;
  padding-top:32rpx;
  position: relative;
  max-width:445rpx;
}
.houseItem .infobox .housetitle{
  width:100%;
  font-size:34rpx;
  font-weight:400;
  color:#333333;
  line-height:45rpx;
  min-height:80rpx;
  margin-bottom:10rpx;
  overflow:hidden;
  word-wrap: break-word;
  word-break: break-all;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
}
.houseItem .infobox .text{
  font-size: 26rpx;
  line-height: 1;
  color:#888888;
  padding-top:5rpx;
  padding-bottom: 10rpx;
}
.houseItem .infobox .newTag text{
  /*color:#999999!important;*/
  /*background: #fff!important;*/
  /*padding:4rpx!important;*/
  /*border-radius: 4rpx!important;*/
  /*border:2rpx solid #999999!important;*/
}
.houseItem .infobox .tag text{
  font-size: 18rpx;
  display: inline-block;
  line-height: 1;
  color: #1bc964;
  background: #e1f7e8;
  padding: 6rpx 6rpx 4rpx;
  border-radius: 30rpx;
  margin-right: 8rpx;
}
.houseItem .infobox .tag .true_flag{
  position: relative;
  display: inline-block;
  top: 5rpx;
  width:81rpx;
  padding-top:21rpx;
  background:url(http://uuweb.haofang.net/Public/wxApp/images/index/true_house_flag.png) center bottom no-repeat;
  background-size:81rpx 21rpx;
  height:38rpx;
  line-height:19rpx;
  padding:0;
  margin-right:10rpx;
}
.houseItem .infobox .tag .owner_flag,.houseItem .infobox .tag .month_flag{
  font-size: 18rpx;
  display: inline-block;
  line-height: 1;
  padding: 6rpx 6rpx 4rpx;
  border-radius: 30rpx;
  margin-right:8rpx;
  color: #ff580a!important;
  background: #ffeade!important;
}

.houseItem .infobox .tag text:nth-of-type(4n+1){
  color: #18acf0;
  background: #ddf3fd;
}
.houseItem .infobox .tag text:nth-of-type(4n+2){
  color: #ff9600;
  background: #fff1dc;
}
.houseItem .infobox .tag text:nth-of-type(4n+3){
  color: #f3685f;
  background: #fde8e7;
}
.houseItem .infobox .tag text:nth-of-type(4n+4){
  color: #1bc964;
  background: #ddf7e8;
}

.houseItem .infobox .price{
  font-size: 30rpx;
  color:#ff5a1f;
  font-weight: bold;
  line-height: 61rpx;
}
.houseItem .infobox .price text{
  color:#999999;
  font-weight: normal;
  font-size: 22rpx;
  margin-left: 18rpx;
}
.houseItem .infobox .top{
  font-size: 16rpx;
  letter-spacing: 1px;
  color:#a5a5a5;
  padding:6rpx;
  border: 1px solid #dcdcdc;
  border-radius: 3rpx;
  position: absolute;
  line-height: 1;
  right:0;
  bottom: 12rpx;
}
.houseItem .infobox .housetitle text{
  font-family: '微软雅黑';
  line-height: 1;
  margin-left: 12rpx;
  display: inline-block;
  position: relative;
  color:#fff;
  font-size:20rpx;
  padding:5rpx 7rpx;
  font-weight:normal;
  top:-2rpx;
}
.houseItem .infobox .housetitle .wait{
  background: #14c4f9;
}
.houseItem .infobox .housetitle .selling{
  background: #0ac95e;
}
.houseItem .infobox .housetitle .soldOut{
  background: #cccccc;
}

.houseItem #tour{
  width: 99rpx;
  height: 33rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: contain;
  position: absolute;
  top:-1rpx;
  left:-2rpx;
}
.houseItem #new{
  width:100rpx;
  height: 53rpx;
  background: url(http://cdn.haofang.net/static/uuminiapp/search/hot_newhouse_logo.png) no-repeat center center;
  background-size: contain;
  position: absolute;
  top: -5rpx;
  left: 13rpx;
}

.houseItem.navigator-hover{
  background-color: #fff;
  opacity: 1;
}
