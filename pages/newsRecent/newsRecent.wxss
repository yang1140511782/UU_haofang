page{
  margin: 0;
  padding: 0;
  background-color: #f6f6f6;
}
.content-list{
  width: 100%;
  overflow: hidden;
}
.item{
  width: 100%;
  height: 120rpx;
  box-sizing: border-box;
  position: relative;
  display: flex;
  padding: 17rpx 26rpx;
  background-color: #fff;
  border-bottom: 1px solid #f9f9f9;
}
.user-face{
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right:33rpx;
  margin-left:12rpx;
  /*background: #fafafa url() center center no-repeat;*/
  /*background-size: 100% 100%;*/
}
.service{
  width:90rpx;
  height:90rpx;
  overflow:hidden;
  margin-right:33rpx;
  box-sizing:border-box;

margin-left:12rpx;
border-radius: 6rpx;

}
.user-face image{
  width: 100%;
  height: 100%;
}
.service image{
  width: 100%;
  height: 100%;
}
.item-right{
  width:566rpx;
}
.username{
  font-size: 28rpx;
  letter-spacing: 1px;
  height: 60rpx;
  line-height: 60rpx;
  color:#343434;
  position: relative;
  width:100%;
}
.username text{
  position: absolute;
  letter-spacing: normal;
  display: inline-block;
  top:0;
  bottom: 0;
  right:0;
  font-size: 21rpx;
  color:#999999;
}
.last-msg{
  font-size: 22rpx;
  color: #989898;
  width: 85%;
  height: 29rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.avatar{
  position: relative;
}
.redDot{
  width:18rpx;
  height: 18rpx;
  background: #fb0a00;
  border-radius: 50%;
  position: absolute;
  z-index: 999;
  top:5rpx;
  right:13rpx;
}