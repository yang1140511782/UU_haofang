.dd_info_r {
    padding-top: 0;
    padding-bottom: 0;
    padding-right: 0;
    margin-top: 30rpx;
}
.dd_info_r button {
    display: inline-block;
    width: 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    color: #ff9600;
    font-size: 28rpx;
    border: 1rpx solid #ff9600;
    border-radius: 8rpx;
    margin-top: 14rpx;
}
.dd_info_r p {
    font-size: 30rpx;
    color: #979797;
}
.dd_info_hd {
    width: 104rpx;
    height: 104rpx;
    margin: 30rpx 30rpx 0 0;
}
.dd_info_ul>li { margin-left: 40rpx; }

/*按钮*/
.btn{
	height:110rpx;
    line-height: 110rpx;
    margin: 42rpx 40rpx 0;
}
.btn_btn{
	width:100%;
	height:100%;
	background:#cecfd3;
	font-size: 36rpx;
	color:rgb(255,255,255);
