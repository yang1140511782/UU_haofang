/* pages/appointAgent/appointAgent.wxss */
.dd_wrap{
	width:100%;
	padding-top:70rpx;
}
.agent_title h2{
	width:100%;
	height:100%;
	font-size:50rpx;
	color:#000000;
	text-align:center;
}
.fc-b-h{
	color:#c9c9c9;
}


/*经纪人信息*/
#content{
    display: block;
    transform-origin: 0px 0px 0px;
    opacity: 1;
    transform: scale(1, 1);
}
.dd_info {
    width: 360rpx;
    margin: auto;
    margin-top: 40rpx;
}
.dd_img{
	display: block;
    width: 120rpx;
    height: 120rpx;
	margin:auto;
    background-repeat: no-repeat;
	-webkit-background-size: cover;
    border-radius: 60px;
    overflow: hidden;
}
.dd_img image{
    display: block;
    width: 100%;
    height: 100%;
    background: url(https://uuweb.haofang.net/PublicC/images/default_avatar_2.png) center no-repeat;
    background-size: 100% 100%;
}
.jjr_info{
	margin: 24rpx auto 20rpx;
	text-align:center;
}
.jjr_name{
    font-size: 32rpx;
    color: #000000;
    font-weight: bold;
}
.jjr_mobile{
    font-size: 36rpx;
    color: #000000;
    font-weight: bold;
}
.jjr_dname{
	font-size: 28rpx;
    color: #808080;
    padding-left: 16rpx;
	/*padding-top:1px;*/
}
.start_score{
	text-align:center;
	display: -webkit-box;
    -webkit-box-align: center;
}

.dd_score_num{
    text-align: center;
    font-size: 22rpx;
    color: #ffffff;
    background-color: #FFAE00;
    display: inline-block;
    height: 30rpx;
    line-height: 30rpx;
    border-radius: 16rpx;
    width: 48rpx;
    margin-left: 12rpx;
}
.start_score text {
    display:block;
}

.dd_score_img {
    width: 220rpx;
    height: 40rpx;
    background: url(https://uuweb.haofang.net/PublicC/images/score_yellow.png) no-repeat;
    background-size: 100% auto;
    margin-left: 36rpx;
    margin-right: 16rpx;
}
