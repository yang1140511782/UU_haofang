/**/
.dd_prompt {
	position: relative;
    margin-right: 40rpx;
    padding-right: 0;
	padding-left: 24rpx;
    padding-top: 60rpx;
    margin-left: 40rpx;
}
.dd_prompt .dd_prompt_ul .dd_prompt_li {
	position: relative;
    line-height: 2;
    font-size: 30rpx;
    color: #808080;
}
.ui-txt-green{
	color:#1bc964;
}

.dd_prompt .dd_prompt_ul .dd_prompt_li:before {
    position: absolute;
    top: 50%;
    left: -26rpx;
    content: '';
    width: 8rpx;
    height: 8rpx;
    margin-top: -4rpx;
    background-color: #1BC964;
    border-radius: 100%;
}

/*发布委托弹窗*/
.publish-entrust{
  position: fixed;
  top: 0;
  bottom: 0;
  left:0;
  right: 0;
  margin: auto;
  height: 100%;
  width: 100%;
  background: rgba(0,0,0,0.3);
}
.publish-content{
  position: absolute;
  bottom: 0;
  width: 100%;
  background: #fff;
  text-align: center;
}
.publish-entrust-mask{
  position: absolute;
  height: 100%;
  width: 100%;
  top: 0;
  bottom: 0;
}
.publish-li-after{
  height: 10rpx;
  background: #F1F1F1
}
.publish-li{
  font-size: 30rpx;
  color: #333;
  padding: 30rpx 0;
  border-bottom: 1px solid #efefef;
}
.publish-content .publish-li:last-child{
  border-bottom:none;
}
.publish-li-icon-com{
  position: relative;
  padding-left: 60rpx;
}
.publish-li-icon-com::before{
  content: '';
  display:inline-block;
  width: 38rpx;
  height: 38rpx;
  background-size: 100% 100%;
  background-position: center center;
  background-repeat:no-repeat 
}
.cancle-class{
  margin-top: 10rpx;
}
.trust-zufang-icon::before{
  background-image: url('http://cdn.haofang.net/static/uuminiapp/personalCenter/trust-zufang-icon.png');
  position: absolute;
  left: 0;
  top: 0;
}
.trust-maifang-icon::before{
