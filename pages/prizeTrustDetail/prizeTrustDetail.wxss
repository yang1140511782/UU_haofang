}
.no-cj{
  width: 44rpx;
  height: 44rpx;
  background: url(http://cdn.haofang.net/static/uuminiapp/entrustImg/no_cj.png) no-repeat center center;
  background-size: 100%;
  margin-bottom: 10rpx;
}
.cj-icon{
  width: 44rpx;
  height: 44rpx;
  background: url(http://cdn.haofang.net/static/uuminiapp/new_images/cj_icon.png) no-repeat center center;
  background-size: 100%;
  margin-bottom: 10rpx;
}
.tousu-success{
  width: 44rpx;
  height: 44rpx;
  background: url(https://uuweb.haofang.net/PublicC/source/images/publish/tousu-cg.png) no-repeat center center;
  background-size: 100%;
  margin-bottom: 10rpx;
}
.no-tousu-success{
  width: 44rpx;
  height: 44rpx;
  background: url(https://uuweb.haofang.net/PublicC/source/images/publish/pro-tousu-icon.png) no-repeat center center;
  background-size: 100%;
  margin-bottom: 10rpx;
  margin:0 auto;
  margin-bottom: 10rpx;
}
.tousu-fail{
  color: #ff5a1f !important;
  text-align: center;
}
.line-w{
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  top: 47rpx;
  width: 70%;
  height: 2rpx;
  background: #b7b7b7;
}
.line-w-a{
  position: absolute;
  height: 1px;
  left: 0;
  top: 26rpx;
  background: green;
}
.progress-1 .condition-box .line-w-a{
  width:0%;
  background: #4daaf0;
} 
.progress-2 .condition-box .line-w-a{
  width:35%;
  background: #4daaf0;
} 
.progress-3 .condition-box .line-w-a{
  width:65%;
  background: #4daaf0;
} 
 .progress-4 .condition-box .line-w-a{
  width:95%;
  background: #4daaf0;
} 
.condition{
  position: relative;
  z-index: 3;
}
.progress-1 .condition:nth-of-type(1),
.progress-2 .condition:nth-of-type(1),
.progress-2 .condition:nth-of-type(2),
.progress-3 .condition:nth-of-type(1),
.progress-3 .condition:nth-of-type(2),
.progress-3 .condition:nth-of-type(3),
.progress-4 .condition:nth-of-type(1),
.progress-4 .condition:nth-of-type(2),
.progress-4 .condition:nth-of-type(3),
.progress-4 .condition:nth-of-type(4)
{
  color:#4daaf0;
}
 .manager-progress .path-line::after{
  content: '';
  position:absolute;
  display: block;
  top:0rpx;
  left: 0;
  width:100%;
  margin: 0 25rpx;
  height: 2px;
  background: #b7b7b7;
  transform: scale(1,0.5);
  transform-origin: top center;
}
.progress3-1 .condition-box .line-w-a{
  width:0%;
  background: #32bf76;
} 
.progress3-2 .condition-box .line-w-a{
  width:45%;
  background: #32bf76;
} 
.progress3-3 .condition-box .line-w-a{
  width:95%;
  background: #32bf76;
} 
.allgreen .condition-box .line-w-a{
  width:90%;
  background: #32bf76;
} 

