/* 服务评价  */
.evaluate_mask{
  position: relative;
  overflow: hidden;
  background-color: #fff;
}
.evaluate_mask .evaluate_box{
  background: #fff;
  width:100%;
  box-sizing: border-box;
  padding:50rpx;
}
.fee-info-box{
  position: relative;
}
.fee-info-box .fee-info-tt{
  font-size: 32rpx;
  color: #999;
  margin-bottom: 75rpx;
}
.fee-info-box .fee-info-num{
  font-weight: bold;
  font-size: 50rpx;
  text-align: center;
  margin: 0 auto 54rpx;
}
.fee-info-box .fee-info-remark{
  font-size: 32rpx;
  margin: 0 auto 60rpx;
  text-align: center;
  color: #000;
}
.evaluate_mask .evaluate_box .title{
  height: 54rpx;
  line-height: 54rpx;
  font-size: 30rpx;
  color:#999;
  position: relative;
  margin-bottom: 43rpx;
}
.evaluate_mask .evaluate_box .star_box{
  width:402rpx;
  height: 61rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
  margin-top: 20rpx;
}
.evaluate_mask .evaluate_box .star_box view{
  background-size: 100% 100%;
  background-image: url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/ic_star_gray.png);
  background-repeat: no-repeat;
}
.evaluate_mask .evaluate_box .star_box .activestar{
  background-image:url(http://cdn.haofang.net/static/uuminiapp/entrustDetail/ic_star_yellow.png);
}
.evaluate_mask .evaluate_box .star_box view{
  width:61rpx;
  height: 61rpx;
}
.evaluate_mask .evaluate_box .reason_box_remark{
  position: relative;
  width: 100%;
  overflow: hidden;
}
.evaluate_mask .evaluate_box .reason_box_remark textarea{
  background-color: #f1f1f1;
  width: 100%;
  height: auto!important;
  min-height: 126rpx;
  font-size: 28rpx;
  border-radius: 8rpx;
  padding: 15rpx 20rpx;
  box-sizing: border-box;
}
.evaluate_mask .evaluate_box .reason_box_remark .reason-remark-text-placeholder{
  font-size: 28rpx;
  color: #888;
  background-color: #f1f1f1;
}
.evaluate_mask .evaluate_box .reason_box_remark view{
  font-size: 26rpx;
  position: absolute;
  right: 15rpx;
  bottom: 18rpx;
  color: #888;
}
.evaluate_mask .evaluate_box .reason_box{
  width:520rpx;
  margin: 0 auto;
  display: flex;
  flex-wrap:wrap;
  margin-bottom: 70rpx;
  margin-top:40rpx;
}
.evaluate_mask .evaluate_box .reason_box .reason{
  width:230rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  box-sizing: border-box;
  text-align: center;
  border: 1rpx solid #ddd;
  border-radius: 4rpx;
  color:#999;
  position: relative;
  box-sizing: border-box;
}
.evaluate_mask .evaluate_box .reason_box .reason:nth-child(1),
.evaluate_mask .evaluate_box .reason_box .reason:nth-child(3){
  margin-right: 60rpx;
}
.evaluate_mask .evaluate_box .reason_box .reason:first-child{
  margin-bottom: 26rpx;
}
.evaluate_mask .evaluate_box .reason_box .reason.select{
  border: 1rpx solid #333;
  color: #333;
  background-color: #f6f6f6;
}
.evaluate_mask .evaluate_box .evaluate_btn{
  height: 94rpx;
  line-height: 94rpx;
  border-radius: 8rpx;
  text-align: center;
  color:#fff;
  background: #cecfd3;
  font-size: 32rpx;
  letter-spacing: 1rpx;
  margin-top: 26rpx;
}
.evaluate_mask .evaluate_box .evaluate_btn.active{
  background: #25b764;
}

.evaluate_mask .evaluate_box .realOrFake .opt .realfakechecked{
  border-color:#25b764;
}
.evaluate_mask .evaluate_box .reason_box .checkedReasonItem view{
  background-image:url(https://uuweb.haofang.net/PublicC/images/houseOrder_xuanzhong.png);
  background-size: 100% 100%;
  border-color:#25b764;
}
